path	upd1	{
	DBUPDATE		{
		UPDATE	tbl_chk
				SET
	CHKKBN		=	:CHKKBN,
	SRYCD		=	:SRYCD,
	YUKOSTYMD	=	:YUKOSTYMD,
	YUKOEDYMD	=	:YUKOEDYMD,
	CDKBN		=	:CDKBN,
	RENNUM		=	:RENNUM,
	TEKCDNUM	=	:TEKCDNUM,
	CD1		=	:CD1,
	CD2		=	:CD2,
	CD3		=	:CD3,
	CD4		=	:CD4,
	CD5		=	:CD5,
	CD6		=	:CD6,
	CD7		=	:CD7,
	CD8		=	:CD8,
	CD9		=	:CD9,
	CD10		=	:CD10,
	TERMID		=	:TERMID,
	OPID		=	:OPID,
	CREYMD		=	:CREYMD,
	UPYMD		=	:UPYMD,
	UPHMS		=	:UPHMS
			WHERE	CHKKBN		=	:UPCHKKBN
			AND		SRYCD		=	:UPSRYCD
			AND		YUKOSTYMD	=	:UPYUKOSTYMD
			AND		YUKOEDYMD	=	:UPYUKOEDYMD
			AND		CDKBN		=	:UPCDKBN
			AND		RENNUM		=	:UPRENNUM
		;
	};
};

