	DBSELECT	{
		DECLARE tbl_ptsrh_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_ptsrh
	        WHERE	HOSPNUM    =    :HOSPNUM
		AND     TERMID     =    :TERMID
