tbl_rececom {
	HOSPID				char(24);
	PTID				number(10);
	NYUGAIKBN			char(1);
	SRYKA				char(2);
	SRYYM				char(6);
	HKNCOMBI			number(4);
	SRYDD				char(2);
	SJKBN				char(2);
	COMMENT        		varchar(1600);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD    			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPID,PTID,NYUGAIKBN,SRYKA,SRYYM,HKNCOMBI,SRYDD,SJKBN;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_rececom_key_csr CURSOR FOR
		SELECT *
		FROM tbl_rececom
			WHERE   HOSPID    =   :HOSPID
			AND     PTID      =   :PTID
			AND     NYUGAIKBN =   :NYUGAIKBN
			AND     SRYKA     =   :SRYKA
			AND     SRYYM     =   :SRYYM
			AND     HKNCOMBI  =   :HKNCOMBI
			AND     SRYDD     =   :SRYDD
			AND     SJKBN     =   :SJKBN
		;
	};
};

