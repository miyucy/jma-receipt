tbl_recekanri	{
        PRTID               char(4);
        SYOKBN              char(1);
        CREYMD              char(8);
        CREHMS              char(6);
        TEISYUTUSAKI        char(1);
        RECESYUBETU         char(4);
        NYUGAIKBN           char(1);
        SRYYM               char(6);
        PAGE                number(5);
        KENSU               number(5);
        RECEKBN             char(1);
        TERMID              varchar(16);
};

primary	{
	PRTID,SYOKBN,CREYMD,CREHMS,TEISYUTUSAKI,RECESYUBETU;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_recekanri_key_csr CURSOR FOR
		SELECT *
		FROM tbl_recekanri
                    WHERE   PRTID        =   :PRTID
                    AND     SYOKBN       =   :SYOKBN
                    AND     CREYMD       =   :CREYMD
                    AND     CREHMS       =   :CREHMS
                    AND     TEISYUTUSAKI =   :TEISYUTUSAKI
                    AND     RECESYUBETU  =   :RECESYUBETU
                    ORDER BY  PRTID,SYOKBN,CREYMD,CREHMS,TEISYUTUSAKI,RECESYUBETU
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_recekanri_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_recekanri
                    WHERE   PRTID        =   :PRTID
                    AND     SYOKBN       =   :SYOKBN
                    ORDER BY  CREYMD DESC,CREHMS DESC,TEISYUTUSAKI,RECESYUBETU
		;
	};
};

