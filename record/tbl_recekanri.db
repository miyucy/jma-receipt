path	key8		{ 	
	DBSELECT	{
		DECLARE tbl_recekanri_key8_csr CURSOR FOR
		SELECT  PREFKBN_SRT,HKNJANUM,SUM(PAGE) AS PAGE,SUM(KENSU) AS KENSU,SUM(PAGE_CHOUKI) AS PAGE_CHOUKI,SUM(PAGE_ZAITAKU) AS PAGE_ZAITAKU
		FROM tbl_recekanri
                    WHERE   PRTID        =   :PRTID
                    AND     SYOKBN       =   :SYOKBN
                    AND     CREYMD       =   :CREYMD
                    AND     CREHMS       =   :CREHMS
                    AND     TEISYUTUSAKI =   :TEISYUTUSAKI
                    AND     NYUGAIKBN    =   :NYUGAIKBN
                    AND     RECESYUBETU  <>  '0000'
                    AND     RECESYUBETU  <>  '8888'
                    AND     RECESYUBETU  <>  '6666'
                    GROUP BY PREFKBN_SRT,HKNJANUM_SRT,HKNJANUM
                    ORDER BY PREFKBN_SRT,HKNJANUM_SRT,HKNJANUM
		;
	};
};

