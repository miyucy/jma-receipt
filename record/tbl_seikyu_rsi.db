tbl_seikyu_rsi	{
HOSPNUM				number(2,0);
SRYYM				number(6);
NYUGAIKBN			char(1);
PTID				number(10);
RECEKA				char(2);
TEISYUTUSAKI			number(1,0);
RECESYUBETU			char(4);
HKNJANUM			char(8);
HOJOKBN_KEY     		char(1);
KOHID_KEY       		number(10);
TEKSTYMD			char(8);
RECEIPT_KBN			char(10);
HKNKBN				char(1);
RECEKBN				char(1);
SYOKAIFLG			char(1);
SYOCD				char(5);
KOFUNUM  			char(14);
SAIGAIKBN			char(1);
COMMENT  			varchar(200);
#-----> 検索用項目
XXTEISYUTUSAKI			number(1,0),virtual;
};

primary	{
	HOSPNUM,SRYYM,NYUGAIKBN,PTID,RECEKA,TEISYUTUSAKI,RECESYUBETU,HKNJANUM,HOJOKBN_KEY,KOHID_KEY,TEKSTYMD,RECEIPT_KBN;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_seikyu_rsi_key_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu_rsi
                WHERE   HOSPNUM      =   :HOSPNUM
                  AND   SRYYM       =   :SRYYM
                  AND   NYUGAIKBN   =   :NYUGAIKBN
                  AND   PTID        =   :PTID
                  AND   RECEKA      =   :RECEKA 
                  AND   TEISYUTUSAKI =  :TEISYUTUSAKI
                  AND   RECESYUBETU =   :RECESYUBETU
                  AND   HKNJANUM    =   :HKNJANUM
                  AND   HOJOKBN_KEY =   :HOJOKBN_KEY
                  AND   KOHID_KEY   =   :KOHID_KEY 
                  AND   TEKSTYMD    =   :TEKSTYMD
                  AND   RECEIPT_KBN    =   :RECEIPT_KBN
		;
	};
};

path	del		{ 	
	DBDELETE	{
		DELETE
		FROM tbl_seikyu_rsi
                WHERE   HOSPNUM      =   :HOSPNUM
                  AND   SRYYM       =   :SRYYM
                  AND   NYUGAIKBN   =   :NYUGAIKBN
                  AND   PTID        =   :PTID
                  AND   RECEKA      =   :RECEKA 
                  AND   TEISYUTUSAKI =  :TEISYUTUSAKI
                  AND   RECESYUBETU =   :RECESYUBETU
                  AND   HKNJANUM    =   :HKNJANUM
                  AND   HOJOKBN_KEY =   :HOJOKBN_KEY
                  AND   KOHID_KEY   =   :KOHID_KEY 
                  AND   TEKSTYMD    =   :TEKSTYMD
                  AND   RECEIPT_KBN    =   :RECEIPT_KBN
		;
	};
};

