    HKNCOMBI_10         number(4,0);
    TENSU               number(9);
    KAISU               number(3);
    NISSU               number(3);
    EDDAY               number(2);
#  Ì¤ÀÁµá¥ì¥»¤Ç»È¤¦Í½Äê
    MISEIKYU_FLG        char(2);
#  ºï½ü¥Õ¥é¥°
    DLT_FLG             char(1);
