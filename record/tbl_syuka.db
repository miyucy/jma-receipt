path	key5		{ 	
	DBSELECT	{
		DECLARE tbl_syuka_key5_csr CURSOR FOR
		SELECT a.ptid
		FROM tbl_syuka a,tbl_ptnum b
			WHERE   a.HOSPNUM       =   :HOSPNUM
			AND     SRYYM          =   :SRYYM
			AND     NYUGAIKBN      =   :NYUGAIKBN
			AND     TEISYUTUSAKI   =   '1'
			AND     a.PTID         =   b.PTID
            AND     a.HOSPNUM       =   b.HOSPNUM
			GROUP BY a.PTID,PTNUM
			ORDER BY PTNUM
		;
	};
};

