tbl_byomei {
	BYOMEICD			char(7);
	BYOMEI				varchar(200);
	BYOMEIMOJI			number(3);
	TANBYOMEI			varchar(200);
	TANBYOMEIMOJI			number(3);
	BYOMEIKANA			varchar(200);
	IKOSAKICD			varchar(7);
	TOKSKNCD			number(2);
	NANBYOCD			number(2);
	UTAGAIFLG			char(1);
	TANDOKUKBN			number(2);
	HKNSKYKBN			number(1);
	HYOJUNCD        		varchar(8);
	SAITAKUKBN			char(1);
	BYOMEICHGCD			varchar(4);
	ICD10_1   			varchar(5);
	ICD10_2   			varchar(5);
	SYUSAIYMD			varchar(8);
	CHGYMD				varchar(8);
	HAISIYMD			varchar(8);
	TERMID  			varchar(32);
	OPID    			varchar(16);
	CREYMD  			varchar(8);
	UPYMD    			varchar(8);
	UPHMS				varchar(6);
};
primary	{
	BYOMEICD;
};

path	key	{ 	
	DBSELECT	{
		DECLARE tbl_byomei_key_csr CURSOR FOR
		SELECT *
		FROM tbl_byomei
			WHERE   BYOMEICD    =   :BYOMEICD
		;

	};
};

path	key2	{ 	
	DBSELECT	{
		DECLARE tbl_byomei_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_byomei
			WHERE  (BYOMEI       LIKE :BYOMEI
			OR      BYOMEIKANA   LIKE :BYOMEI)
			ORDER BY             BYOMEIKANA
		;
	};
};

path	key3	{ 	
	DBSELECT	{
		DECLARE tbl_byomei_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_byomei
			WHERE  (BYOMEIKANA   LIKE :BYOMEIKANA)
			ORDER BY             BYOMEI
		;
	};
};

path	key4	{ 	
	DBSELECT	{
		DECLARE tbl_byomei_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_byomei
			WHERE   BYOMEI       =   :BYOMEI
			AND     BYOMEICD     <   'ZZZ0000'
		;
	};
};

