path	key 	{
	DBSELECT	{
		DECLARE tbl_syunou_key_csr CURSOR FOR
		SELECT *
		FROM tbl_syunou
			WHERE   HOSPID    =   :HOSPID
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   DENPNUM     =   :DENPNUM
		;
	};
};

