tbl_chk005	{
	CHKKBN			char(1);
	ID					integer;
	YUKOSTYMD  	char(8);
	YUKOEDYMD  	char(8);
	CDKBN   		char(1);
	RENNUM			integer;
	SRYCD   		char(9);
	CD				varchar(170);
	BYOMEI			varchar(160);
	COMPKBN			char(1);
	BYOMEIHENFLG	char(1);
	NYUGAIKBN		char(1);
	NYUGAIBNDKBN	char(1);
	DAYMONTHKBN		char(1);
	TERMID  		varchar(16);
	OPID    		varchar(16);
	CREYMD  		char(8);
	UPYMD   		char(8);
	UPHMS			char(6);
#-----> 主キー更新用項目
	UPCHKKBN		char(1),virtual;
	UPCHKKBN_RENNUM		integer,virtual;
	UPYUKOSTYMD 	char(8),virtual;
	UPYUKOEDYMD 	char(8),virtual;
	UPCDKBN   		char(1),virtual;
	UPRENNUM		integer,virtual;
#-----> 検索キー用項目
	SELNAME		char(200),virtual;
};

primary	{
	CHKKBN,ID,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_chk005_key_csr CURSOR FOR
		SELECT *
		FROM tbl_chk005
			WHERE CHKKBN      =   :CHKKBN
			AND   ID	  			=   :ID
			AND   RENNUM      =   :RENNUM
		;
	};
};

path	key2		{ 	
	DBSELECT	{
		DECLARE tbl_chk005_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_chk005
			WHERE CHKKBN      =   :CHKKBN
			AND		BYOMEI      =   :BYOMEI
			AND		RENNUM      =   1
			ORDER BY YUKOSTYMD
		;
	};
};

path	key3		{ 	
	DBSELECT	{
		DECLARE tbl_chk005_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_chk005
			WHERE CHKKBN      =   :CHKKBN
			AND   ID	  =   :ID
			ORDER BY RENNUM
		;
	};
};

path	key4		{ 	
	DBSELECT	{
		DECLARE tbl_chk005_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_chk005
			WHERE CHKKBN      =   :CHKKBN
			AND   RENNUM      =   1
			ORDER BY ID    DESC
		;
	};
};

path	del1		{ 	
	DBDELETE	{
		DELETE
		FROM tbl_chk005
			WHERE CHKKBN      =   :CHKKBN
			AND   ID	  =   :ID
		;
	};
};

path	key5	{ 	
	DBSELECT	{
		DECLARE tbl_chk005_key5_csr CURSOR FOR
		SELECT * FROM TBL_CHK005
		WHERE  CHKKBN     = :CHKKBN
		AND YUKOSTYMD    <= :YUKOSTYMD
		AND YUKOEDYMD    >= :YUKOEDYMD
		AND RENNUM        = 1 
		ORDER BY BYOMEI
		;
	};
};

path	key6	{ 	
	DBSELECT	{
		DECLARE tbl_chk005_key6_csr CURSOR FOR
		SELECT * FROM TBL_CHK005
		WHERE  CHKKBN     = :CHKKBN
		AND YUKOSTYMD    <= :YUKOSTYMD
		AND YUKOEDYMD    >= :YUKOEDYMD
		ORDER BY BYOMEI,ID
		;
	};
};
