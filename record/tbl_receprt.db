tbl_receprt	{
        PRTID               char(4);
        RECEYM              char(6);
        CREYMD              char(8);
        CREHMS              char(6);
        PREFKBN             char(1);
        PREFNUM             char(2);
        HKNJANUM            char(8);
        TEKSTYMD            char(8);
        TEISYUTUSAKI        char(1);
        RECESYUBETU         char(4);
        PTNUM               char(20);
        SRYYM               char(6);
        RENNUM              number(2);
        RECENUM             number(6);
        HKNKBN              char(1);
        KANANAME            char(100);
        TERMID              char(32);
        PRTJYUN             number(2);
        PRTRENNUM           number(6);
        NYUGAIKBN           char(1);
        FORMID              char(20);
        PRTDATA             varchar(10000);
#-----> ¸¡º÷ÍÑ¹àÌÜ
	STKANANAME			char(100),virtual;
	EDKANANAME			char(100),virtual;
};

primary	{
	PRTID,RECEYM,CREYMD,CREHMS,PREFKBN,PREFNUM,HKNJANUM,TEKSTYMD,TEISYUTUSAKI,RECESYUBETU,PTNUM,SRYYM,RENNUM,RECENUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_receprt_key_csr CURSOR FOR
		SELECT *
		FROM tbl_receprt
                    WHERE   PRTID        =   :PRTID
                    AND     RECEYM       =   :RECEYM
                    AND     CREYMD       =   :CREYMD
                    AND     CREHMS       =   :CREHMS
                    AND     TEISYUTUSAKI =   :TEISYUTUSAKI
                    AND     RECESYUBETU  =   :RECESYUBETU
                    AND     PREFKBN      =   :PREFKBN
                    AND     KANANAME     >=  :STKANANAME
                    AND     KANANAME     <=  :EDKANANAME 
                   ORDER BY  PREFKBN,PREFNUM,HKNKBN,HKNJANUM,KANANAME,PTNUM,SRYYM,RENNUM
		;
	};
};

