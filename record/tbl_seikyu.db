		;
	};
};

path	key3	{
	DBSELECT	{
		DECLARE tbl_seikyu_key3_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu
                WHERE (      HOSPID      =   :HOSPID     )
                  AND (      HKNNUM_S    <>  '060'       )
                  AND (      HKNNUM_S    <>  '067'       ) 
                  AND (      RECESYUBETU <>  '0000'      )
                  AND (   (( SRYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )
                       AND (( SKYYM       =  '000000' ) OR ( SKYYM = :SKYYM )))
                       OR (( SKYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )))
		;
	};
};

path	key4	{
	DBSELECT	{
		DECLARE tbl_seikyu_key4_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu
                WHERE (      HOSPID      =   :HOSPID     )
                  AND (      HKNNUM_S    =  '060'                   
                        OR   HKNNUM_S    =  '067'        ) 
                  AND (      RECESYUBETU <>  '0000'      ) 
                  AND (   (( SRYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )
                       AND (( SKYYM       =  '000000' ) OR ( SKYYM = :SKYYM )))
                       OR (( SKYYM       =   :SKYYM   )
                       AND ( SKYKBN      <>  '0'      )))
                ORDER  BY  HKNJANUM_S   
		;
	};
};

path	key5	{
	DBSELECT	{
		DECLARE tbl_seikyu_key5_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu
                WHERE   HOSPID              =   :HOSPID
                AND     RECESYUBETU         <>  '0000'  
                AND     SRYYM               <=  :SKYYM
                AND   (     ( SKYKBN        <>  '2'       )
                         OR (      SKYYM     =   :SKYYM  
                              AND  SRYYM     <>  :SKYYM   ) )
                ORDER   BY  TEISYUTUSAKI,SRYYM,SKYYM,HKNJANUM,PTNUM,SKYYM     
		;
	};
};

