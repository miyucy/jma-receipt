path	key		{ 	
	DBSELECT	{
		DECLARE tbl_seikyu_key_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu
                WHERE   HOSPID      =   :HOSPID
                  AND   SRYYM       =   :SRYYM
                  AND   NYUGAIKBN   =   :NYUGAIKBN
                  AND   PTID        =   :PTID
                  AND   RECEKA      =   :RECEKA 
                  AND   TEISYUTUSAKI =  :TEISYUTUSAKI
                  AND   RECESYUBETU =   :RECESYUBETU
                  AND   HKNJANUM    =   :HKNJANUM
                  AND   TEKSTYMD    =   :TEKSTYMD
		;
	};
};

path	key2	{
	DBSELECT	{
		DECLARE tbl_seikyu_key2_csr CURSOR FOR
		SELECT *
		FROM tbl_seikyu
                WHERE   HOSPID      =   :HOSPID
                  AND   SRYYM       =   :SRYYM
                  AND   PTID        =   :PTID
