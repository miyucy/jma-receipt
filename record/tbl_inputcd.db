tbl_inputcd {
	HOSPID				char(24);
	CDSYU   			char(1);
	INPUTCD 			char(20);
	SRYKBN				char(2);
	SRYCD				char(9);
	DSPSEQ				number(2);
	DSPNAME				varchar(200);
	TERMID  			varchar(16);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD   			char(8);
	UPHMS				char(6);
};

primary	{
	HOSPID,CDSYU,INPUTCD;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_inputcd_key_csr CURSOR FOR
		SELECT *
		FROM tbl_inputcd
			WHERE   HOSPID     =   :HOSPID
			AND   CDSYU        =   :CDSYU
			AND   INPUTCD      LIKE   :INPUTCD
			ORDER   BY         INPUTCD
		;
	};
};

