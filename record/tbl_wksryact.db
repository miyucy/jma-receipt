};

path	key 	{
	DBSELECT	{
		DECLARE tbl_wksryact_key_csr CURSOR FOR
		SELECT *
		FROM tbl_wksryact
			WHERE   HOSPID    =   :HOSPID
			AND   NYUGAIKBN   =   :NYUGAIKBN
			AND   PTID        =   :PTID
			AND   SRYKA       =   :SRYKA
			AND   SRYYMD      =   :SRYYMD
			AND   HKNCOMBI    =   :HKNCOMBI
			AND   ZAINUM      =   :ZAINUM
			AND   RENNUM      =   :RENNUM
		;
	};
};

