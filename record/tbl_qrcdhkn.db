tbl_qrcdhkn {
	HOSPID				char(24);
	REGISTYMD  			char(8);
	REGISTID    			number(5,0);
	REGISTTIME 			char(6);
	SEIDOKBN			char(3);
	HKNNUM	 			char(8);
	KIGONUM				varchar(20);
	HONKZKKBN			char(1);
	KANANAME			varchar(28);
	SEX				char(1);
	BIRTHDAY			char(7);
	KANRINUM			varchar(100);
	NAME				varchar(100);
	WARIAI				varchar(02);
	SAIHAKKU			varchar(50);
	PTID	   			number(10,0);
	ERRCD    			char(2);
	MOD_FLG				number(1);
	TERMID  			varchar(64);
	OPID    			varchar(16);
	CREYMD  			char(8);
	UPYMD   			char(8);
	UPHMS				char(6);
};
primary	{
	HOSPID,REGISTYMD,REGISTID;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_qrcdhkn_key_csr CURSOR FOR
		SELECT *
		FROM tbl_qrcdhkn
			WHERE   HOSPID    =   :HOSPID
			AND   REGISTYMD      =   :REGISTYMD
			AND   REGISTID       =   :REGISTID
		;
	};
};

