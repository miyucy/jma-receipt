tbl_para {
	HOSPNUM				number(2,0);
	GYOUMUID			char(3);
	TERMID				char(64);
	FILEMEI 			char(20);
	EDANUM  			number(3,0);
	DATA_REC 			char(5000);
};
primary	{
	HOSPNUM,GYOUMUID,TERMID,FILEMEI,EDANUM;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_para_key_csr CURSOR FOR
		SELECT *
		FROM tbl_para
		WHERE 	HOSPNUM	= :HOSPNUM
			AND   GYOUMUID   =   :GYOUMUID
			AND   TERMID       =   :TERMID
			AND   FILEMEI      LIKE  :FILEMEI
			AND   EDANUM       =   :EDANUM
		;
	};
};

path	key2 	{
