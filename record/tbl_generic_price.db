tbl_generic_price {
	YAKKAKJNCD			char(9);
	YUKOSTYMD			char(8);
	YUKOEDYMD			char(8);
	PRICE   			number(11,2);
	CREYMD   			char(8);
	UPYMD    			char(8);
};
primary	{
	YAKKAKJNCD,YUKOSTYMD,YUKOEDYMD;
};

path	key 	{
	DBSELECT	{
		DECLARE tbl_generic_price_key_csr CURSOR FOR
		SELECT *
		FROM tbl_generic_price
		WHERE   YAKKAKJNCD    =   :YAKKAKJNCD
			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

path	all	{
	DBSELECT	{
		DECLARE tbl_generic_price_all_csr CURSOR FOR
		SELECT *
		FROM tbl_generic_price
 		;
	};
};
