path	upd1	{ 	
	DBUPDATE	{
		UPDATE	tbl_syskanri 
				SET	
#					:*
					KANRICD      =   :KANRICD,
					KBNCD     	 =   :KBNCD,
					STYUKYMD     =   :STYUKYMD,
					EDYUKYMD     =   :EDYUKYMD,
					KANRITBL     =   :KANRITBL,
					TERMID       =   :TERMID,
					OPID         =   :OPID,
					CREYMD       =   :CREYMD,
					UPYMD        =   :UPYMD,
					UPHMS        =   :UPHMS
			WHERE   KANRICD      =   :UPKANRICD
			AND		KBNCD        =   :UPKBNCD
			AND		STYUKYMD     =   :UPSTYUKYMD
			AND		EDYUKYMD     =   :UPEDYUKYMD
		;
	};
};
