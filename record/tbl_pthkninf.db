path	del1 	{
	DBDELETE	{
		DELETE
		FROM tbl_pthkninf
			WHERE   HOSPID    =   :HOSPID
			AND   PTID        =   :PTID
		;
	};
};

path	key7 	{
	DBSELECT	{
		DECLARE tbl_pthkninf_key7_csr CURSOR FOR
		SELECT *
		FROM tbl_pthkninf
			WHERE HOSPID      =   :HOSPID
			AND   PTID        =   :PTID
			AND   HKNJANUM    =  :HKNJANUM
			AND   SAKJOKBN    <> '1'
			ORDER   BY      HKNID
		;
	};
};
