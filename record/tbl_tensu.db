path	key 	{
	DBSELECT	{
		DECLARE tbl_tensu_key_csr CURSOR FOR
		SELECT *
		FROM tbl_tensu
			WHERE   SRYCD    =   :SRYCD
 			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
		;
	};
};

