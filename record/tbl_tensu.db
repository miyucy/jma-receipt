path	key41 	{
	DBSELECT	{
		DECLARE tbl_tensu_key41_csr CURSOR FOR
		SELECT *
		FROM tbl_tensu
			WHERE   (NAME       LIKE :NAME
			OR     KANANAME   LIKE :KANANAME)
			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
			AND   SUBSTR(SRYCD,1,1) <> '6'
			AND   SUBSTR(SRYCD,1,1) <> '7'
			AND  (SRYKBN     <    '60'
			OR   SRYKBN     >    '69'  )
			ORDER   BY       KANANAME
		;
	};
};

