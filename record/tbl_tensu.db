path	key42k	{
	DBSELECT	{
		DECLARE tbl_tensu_key42k_csr CURSOR FOR
		SELECT a.*
		FROM tbl_tensu a,tbl_kensasort b
			WHERE a.SRYCD    =   b.SRYCD
			AND   YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
			AND  (SRYKBN     >=   '60'  
			AND   SRYKBN     <=   '69'  )   
                        AND   b.KNSBUNRUI =  :KNSJISGRPKBN
			ORDER   BY           b.DSPSEQ 
		;
	};
};

path	key42jk	{
	DBSELECT	{
		DECLARE tbl_tensu_key42jk_csr CURSOR FOR
		SELECT distinct a.*,c.DSPSEQ
		FROM tbl_tensu a,tbl_inputcd b,tbl_kensasort c
			WHERE YUKOSTYMD  <=  :YUKOSTYMD
			AND   YUKOEDYMD  >=  :YUKOEDYMD
			AND  (a.SRYKBN     >=   '60'  
			AND   a.SRYKBN     <=   '69'  )
			AND   c.KNSBUNRUI  = :KNSJISGRPKBN
			AND    a.SRYCD     =   b.SRYCD
			AND    a.SRYCD     =   c.SRYCD
			ORDER   BY          c.DSPSEQ
					;
	};
};

