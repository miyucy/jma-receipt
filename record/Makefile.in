all:   

filecheck:
	@(for i in $(DEF_files) $(RECORD_files) $(DB_files) ;do echo "make	$${i}"; done ;\
	find . \
	-name "." -o \
	-name "CVS" -prune -o \
	-name "Makefile*" -o \
	-name ".cvsignore" -o \
	-name "*.bak" -o \
	-printf "file\t%f\n") \
	|sort +1 |uniq -u -f1 

clean: 

distclean: 
	-rm -f Makefile

install: all
	$(INSTALL) -d $(recorddir)
	$(INSTALL_DATA) $(DEF_files) $(RECORD_files) $(DB_files) $(recorddir)
