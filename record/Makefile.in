shell.db: shell.db.in
	sed -e 's,\@scriptsdir\@,$(scriptsdir),g' $< >$@

all: shell.db  

filecheck:
	@(for i in $(DEF_files) $(RECORD_files) $(DB_files) shell.db.in;do echo "make	$${i}"; done ;\
	find . \
	-name "." -o \
	-name "CVS" -prune -o \
	-name "Makefile*" -o \
	-name ".cvsignore" -o \
	-name "*.bak" -o \
	-name "shell.db" -o \
	-printf "file\t%f\n") \
	|sort -k2 |uniq -u -f1 

clean: 
	-rm -f shell.db

distclean: 
	-rm -f shell.db
	-rm -f Makefile

install: all
	$(INSTALL) -d $(recorddir)
	$(INSTALL_DATA) $(DEF_files) $(RECORD_files) $(DB_files) shell.db $(recorddir)
