path	upd1	{
	DBUPDATE		{
		UPDATE	tbl_chksnd
				SET
		CHKKBN		 =	:CHKKBN,
		SRYCD   	 =	:SRYCD,
		YUKOSTYMD  	 =	:YUKOSTYMD,
		YUKOEDYMD  	 =	:YUKOEDYMD,
		CDKBN   	 =	:CDKBN,
		RENNUM		 =	:RENNUM,
		CD			 =	:CD,
		BYOMEI		 =	:BYOMEI,
		COMPKBN		 =	:COMPKBN,
		BYOMEIHENFLG =	:BYOMEIHENFLG,
		TERMID  	 =	:TERMID,
		OPID    	 =	:OPID,
		CREYMD  	 =	:CREYMD,
		UPYMD   	 =	:UPYMD,
		UPHMS		 =	:UPHMS,
	WHERE	CHKKBN		=	:UPCHKKBN
	AND		SRYCD		=	:UPSRYCD
	AND		YUKOSTYMD	=	:UPYUKOSTYMD
	AND		YUKOEDYMD	=	:UPYUKOEDYMD
	AND		CDKBN		=	:UPCDKBN
	AND		RENNUM		=	:UPRENNUM
		;
	};
};

