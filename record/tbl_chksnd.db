#--TEMPテーブルを操作する場合、KEY名の頭に"tmp"をつけること。
path	tmpkey	{ 	
	DBCREATE	{
		CREATE TEMP TABLE tbl_chksnd ON COMMIT PRESERVE ROWS AS
		SELECT *
		FROM   tbl_chksnd
		WHERE  HOSPNUM	  = :HOSPNUM
		;
		
		CREATE INDEX idx_chksnd_tmp ON pg_temp.tbl_chksnd (SRYCD)
		;
	};

	DBDROP	{
		DROP TABLE pg_temp.tbl_chksnd
		;
	};

};

