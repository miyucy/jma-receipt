tbl_chksnd	{
	CHKKBN			char(1);
	SRYCD   		char(9);
	YUKOSTYMD  		char(8);
	YUKOEDYMD  		char(8);
	CDKBN   		char(1);
	RENNUM			number(3);
	CD				varchar(170);
	BYOMEI			varchar(160);
	COMPKBN			char(1);
	BYOMEIHENFLG	char(1);
	NYUGAIKBN		char(1);
	TERMID  		varchar(16);
	OPID    		varchar(16);
	CREYMD  		char(8);
	UPYMD   		char(8);
	UPHMS			char(6);
#-----> 潛平□凳蕙迕嫩杴
	UPCHKKBN		char(1),virtual;
	UPSRYCD   		char(9),virtual;
	UPYUKOSTYMD 	char(8),virtual;
	UPYUKOEDYMD 	char(8),virtual;
	UPCDKBN   		char(1),virtual;
	UPRENNUM		number(3),virtual;
#-----> 腹綢平□迕嫩杴
	SELNAME		char(200),virtual;
};

primary	{
	CHKKBN,SRYCD,YUKOSTYMD,YUKOEDYMD,CDKBN,RENNUM;
};

path	key		{ 	
	DBSELECT	{
		DECLARE tbl_chksnd_key_csr CURSOR FOR
		SELECT *
		FROM tbl_chksnd
			WHERE CHKKBN      =   :CHKKBN
			AND   SRYCD       =   :SRYCD
			AND   YUKOSTYMD  <=   :YUKOSTYMD
			AND   YUKOEDYMD  >=   :YUKOEDYMD
			AND   CDKBN       =   :CDKBN
			AND   RENNUM      =   :RENNUM
		;
	};
};

