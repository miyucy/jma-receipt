#!/bin/sh

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

ORCASCRIPT=$ORCA_DIR/scripts/claim/rb
#-------------------------------------------#
#  $1 - InputXMLFile
#  $2 - Output SequencialFile
#-------------------------------------------#
#-------------------------------------------#
# XML File Exchange Execute
# 
#-------------------------------------------#
  cd $ORCASCRIPT
     ./decode_xml.rb -e $1 ../def/orca_layout_decode.def ../link/link_decode.def $2
#-------------------------------------------#
#        $2 SequencialFile
#-------------------------------------------#
       $DBSTUB -dir "$LDDEFDIR"/directory -bd orcabt ORCL0030 -parameter $2 >/tmp/L0030.log
	rm ${2:0:31}*
       exit 
