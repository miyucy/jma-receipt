#!/bin/bash

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

if [ ${MCP_MIDDLEWARE_NAME} = "panda" ]
  then
ORCASCRIPT=$ORCA_DIR/scripts/claim/rb
#-------------------------------------------#
#        $1 KBN        X(01)
#        $2 PTID       9(10)
#        $3 SRYKA      X(02)      
#        $4 TIME       X(06) 
#        $5 HOSPNUM    X(02)
#        $6 IP-ADDRESS X(05)
#        $7 PORT       X(05)
#        $8 DRCD       X(05)
#        $9 HKNCOMBI   X(04)
#        $10 SINRYONAIYO   X(16)
#        $11 RENNUM        X(02)
#        $12 REGIST YMD    X(08)
#        $13 MEMO INFO     X(02)
#-------------------------------------------#
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCL0010 -parameter $1,$2,$3,$4,$5,$8,$9,${10},${11},${12},${13},END 

#-------------------------------------------#
# Add Start (2001/08/27)
#-------------------------------------------#
# Make XML File Script Execute
#   orca_layout_front.def - Layout Define File
#   claim$2$3.txt         - Input Sequential File
#   orca_base_front.xml   - Template XML File
#   orca_send_front.xml   - Output XML File
  cd $ORCASCRIPT
     ./orca_make_xml.rb ../def/orca_layout_front.def /var/tmp/claim$5$2$3.txt ../template/orca_base_front.xml >/var/tmp/claim_send_front$5$2$3.xml
#-------------------------------------------#
# Add End
#-------------------------------------------#

#-------------------------------------------#
# Add Start (2001/10/29)
#-------------------------------------------#
if [ $1 = "1" ]
  then
    # --- to EUC  ---
	./clm_snd.rb $6 $7 /var/tmp/claim_send_front$5$2$3.xml euc 
elif [ $1 = "2" ]
  then
    # --- to SJIS  ---
	./clm_snd.rb $6 $7 /var/tmp/claim_send_front$5$2$3.xml sjis 
elif [ $1 = "3" ]
  then
    # --- to UTF-8 ---
	./clm_snd.rb $6 $7 /var/tmp/claim_send_front$5$2$3.xml u8
fi
#-------------------------------------------#
RC='0'$?
if [ ${11} = "01" ]
  then
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCL0012P -parameter $1,$2,$3,$4,$5,$8,$9,${RC:${#RC}-2:2},${12},END 
fi
#-------------------------------------------#
	rm /var/tmp/claim*$5$2$3*
#-------------------------------------------#
# Add End
#-------------------------------------------#
fi
#-------------------------------------------#
if [ ${MCP_MIDDLEWARE_NAME} = "ginbee" ]
  then
# API PUSH start
#-------------------------------------------#
PUSH_FILE="push_accept01.rec"
CLAIM_PUSH=""
MONPUSHEVENT="/usr/lib/panda/bin/monpushevent"
UUID=`uuidgen`
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCL0040 -parameter $1,$2,$3,$4,$5,$8,$9,${10},${11},${12},${13},${14},$UUID,END 
#	${MCP_TEMPDIR}/${Uid}
#
	echo "MCP_MIDDLEWARE_NAME = " ${MCP_MIDDLEWARE_NAME}
	if  test -e $PATCHRECORDDIR/$PUSH_FILE; then
		CLAIM_PUSH="$PATCHRECORDDIR/$PUSH_FILE"
		echo "patch hit!!"
	else
	  if  test -e $RECORDDIR/$PUSH_FILE; then
		CLAIM_PUSH="$RECORDDIR/$PUSH_FILE"
 		echo "org hit!!"
	  fi
	fi
#
	$MONPUSHEVENT $CLAIM_PUSH ${MCP_TEMPDIR}/${UUID}
#-------------------------------------------#
# API PUSH end
#-------------------------------------------#
fi
        exit 
