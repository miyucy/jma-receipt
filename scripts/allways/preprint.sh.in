#!/bin/bash

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV
#-------------------------------------------#
#    印刷ＤＢからの印刷処理
#        $1-${11} 印刷ＤＢ用固定引数
#        ${12} 未設定
#        ${13} ジョブＩＤ
#        ${14} シェルＩＤ
#        ${15} 印刷開始頁
#        ${16} 印刷終了頁
#-------------------------------------------#
#
	echo $#
        
    if [ "${17}" == "1" ]; then
      ${MONUPLOAD} -t report -p ${16} ${15}
    else
      lpr -P ${16} ${15}
    fi

        exit 
