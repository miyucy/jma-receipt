#!/bin/bash

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV
#-------------------------------------------#
#    自賠責（入院）請求書用ファイル作成
#        CPCOMMONSHELL1.INC  
#        $1 SRYYM TERID SYSYMD
#        $2 SYOKBN
#           1:一括作成  2:個別作成
#        $3 RECEKBN
#           3:全体  
#        $4 エラーファイル名 
#        $5 JOBID 
#        $6 SHELLID
#        $7 県単用項目編集プログラム名（医保用）
#        $8 処理年月
#        $9  作成年月日
#        $10 作成時間
#        $15 枠記載区分
#        $16 HOSPNUM 
#        $17 様式区分
#            0:旧様式、1:平成１９年４月改正様式、2:ＯＣＲ様式
#        $18 レセプト作成区分
#            1:提出用レセ、2:点検用レセ
#        $19 請求データ削除区分
#            0:削除しない、2:削除する
#-------------------------------------------#
##      エラーファイル削除
	echo $#
	echo "echo " $4
        if  [ -e $4 ]; then
            rm  $4
        fi
        
        rm  -f /var/tmp/RECE8*
        rm  -f /var/tmp/RECEYMD*
        
        rm  -f /tmp/${16}00RECE8*
        rm  -f /tmp/${16}00RECEYMD*

        if [ ${2} -eq  '1' ]; then
           LOG_FILE="/var/log/jma-receipt/${16}recept11a.log"
        else
           LOG_FILE="/var/log/jma-receipt/${16}recept11b.log"
        fi
        
    echo "arg1 =  [" ${1} "]" >$LOG_FILE
    echo "arg2 =  [" ${2} "]" >>$LOG_FILE
    echo "arg5 =  [" ${5} "]" >>$LOG_FILE
    echo "arg6 =  [" ${6} "]" >>$LOG_FILE
    echo "arg8 =  [" ${8} "]" >>$LOG_FILE
    echo "arg9 =  [" ${9} "]" >>$LOG_FILE
    echo "arg10 = [" ${10} "]" >>$LOG_FILE
    echo "arg15 = [" ${15} "]" >>$LOG_FILE
    echo "arg16 = [" ${16} "]" >>$LOG_FILE
    echo "arg17 = [" ${17} "]" >>$LOG_FILE
    echo "arg18 = [" ${18} "]" >>$LOG_FILE
    echo "arg19 = [" ${19} "]" >>$LOG_FILE

    	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0800 -parameter $1$2$3$5$6,${17},${16},${4}    >> $LOG_FILE 2>&1
	
        if  [ -e $4 ]; then
            exit  
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0810 -parameter $1$2$3$5$6,${17},${16},${4}    >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit  
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0820 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi 

        if  [ -e $4 ]; then
            exit 
        else 
    	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0830 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
     	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0840 -parameter $1$3$5$6,${17},${18},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0850 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0860 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0870 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0880 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0890 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
           exit 
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0900 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0910 -parameter $1$3$5$6,${17},${18},${16},${4}    >> $LOG_FILE 2>&1

        fi

        if  [ ${17} -eq '2' ]; then
            if  [ -e $4 ]; then
                exit 
            else 
                $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0911 -parameter $1$3$5$6,${17},${18},${16},${4}  >> $LOG_FILE 2>&1
            fi
        fi

        if  [ -e $4 ]; then
            exit 
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0920 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0935 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e $4 ]; then
            exit 
        else 
       	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0940 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ ${17} -eq '2' ]; then
            if  [ -e $4 ]; then
                exit 
            else 
                $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0937 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1
            fi
        fi

        if  [ -e $4 ]; then
            exit 
        else 
           if  [ ${17} -eq '0' ]; then
               if  [ ${15} -eq '0' ]; then
                   $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0955 -parameter $9${10}$1$2$3$8$5$6,${18},${19},${16},${4}   >> $LOG_FILE 2>&1
               else
                   $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0957 -parameter $9${10}$1$2$3$8$5$6,${18},${19},${16},${4}   >> $LOG_FILE 2>&1
               fi
           else
               if  [ ${17} -eq '1' ]; then
                   $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0965 -parameter $9${10}$1$2$3$8$5$6,${15},${18},${19},${16},${4}  >> $LOG_FILE 2>&1
               else
                   $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0967 -parameter $9${10}$1$2$3$8$5$6,${15},${18},${19},${16},${4}  >> $LOG_FILE 2>&1
               fi
           fi
        fi

        if  [ -e $4 ]; then
            exit 
        else
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCBJOB -parameter JBE$5$6,${16}        >> $LOG_FILE 2>&1
        
            rm  -f /tmp/${16}00RECE8*
            rm  -f /tmp/${16}00RECEYMD*

        fi

        exit
