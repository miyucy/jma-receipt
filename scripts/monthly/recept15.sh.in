#!/bin/bash

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

#-------------------------------------------#
#    公害レセプト作成（入院）
#        $1  SRYYM TERID SYSYMD
#        $2  SYOKBN
#            1:一括作成  2:個別作成
#        $3  RECEKBN
#            6:公害
#        $4  エラーファイル名 
#        $5  JOBID 
#        $6  SHELLID
#        $8  処理年月
#        $9  作成年月日
#        $10 作成時間
#        $16 HOSPNUM 
#        $18 レセプト作成区分
#            1:提出用レセ、2:点検用レセ
#-------------------------------------------#
##      エラーファイル削除
	echo $#
	echo "echo " $4
        rm  -f $4
        
        rm  -f /tmp/${16}00RECE7*

        if [ ${2} -eq  '1' ]; then
           LOG_FILE="/var/log/jma-receipt/${16}recept15a.log"
        else
           LOG_FILE="/var/log/jma-receipt/${16}recept15b.log"
        fi

	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0600 -parameter $1$2$3$5$6,${16},${4} > $LOG_FILE 2>&1
        
        if  [ -e $4 ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1200 -parameter $1$2$3$5$6,${16},${4} >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1210 -parameter $1$5$6,${16},${4} >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1220 -parameter $1$5$6,${18},${16},${4} >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1230 -parameter $1$5$6,${16},${4} >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1240 -parameter $1$5$6,${16},${4} >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1250 -parameter $1$5$6,${16},${4} >> $LOG_FILE 2>&1
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
	        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1260 -parameter $1$5$6,${16},${4} >> $LOG_FILE 2>&1
        fi

        if  [ -e $4 ]; then
            exit 
        else 
            $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR1270 -parameter $9${10}$1$2$3$8$5$6,${18},${16},${4} >> $LOG_FILE 2>&1
        fi
        
        if  [ -e $4 ]; then
            exit 
        else
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCBJOB -parameter JBE$5$6,${16} >> $LOG_FILE 2>&1
        
            rm  -f /tmp/${16}00RECE7*
	fi
	
        exit 
