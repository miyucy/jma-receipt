#!/bin/sh

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV

#-------------------------------------------#
#    医療請求書用ファイル（入院）作成
#
#        CPCOMMONSHELL1.INC  
#        $1 SRYYM TERID SYSYMD
#        $2 SYOKBN
#           1:一括作成  2:個別作成
#        $3 RECEKBN
#           0:全体  1:社保　2:国保
#        $4 エラーファイル名 
#        $5 JOBID 
#        $6 SHELLID
#        $7 県単用項目編集プログラム名
#        $8  処理年月
#        $9  作成年月日
#        $10 作成時間
#        $11 県番号（個別作成のときは"00"）
#        $12 社保レセ電出力区分
#        $13 国保レセ電出力区分
#        $14 県区分（個別作成のとき山形、福岡のみ設定）
#-------------------------------------------#
##      エラーファイル削除
	echo $#
	echo "echo " $4
        if  [ -e $4 ]; then
            rm  $4
        fi
        
        rm  /var/tmp/RECE6*
        

	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0600 -parameter $1$2$3$5$6    > /home/orca/recept9.log
	
        if  [ -e $4 ]; then
            exit  
        else 
            $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0610 -parameter $1$2$3$5$6    >> /home/orca/recept9.log
	fi

        if  [ -e $4 ]; then
            exit  
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0620 -parameter $1$3$5$6      >> /home/orca/recept9.log
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0630 -parameter $1$5$6        >> /home/orca/recept9.log
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
	   $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0640 -parameter $1$5$6        >> /home/orca/recept9.log
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0650 -parameter $1$5$6        >> /home/orca/recept9.log
        fi

        if  [ -e $4 ]; then
            exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0655 -parameter $1$5$6        >> /home/orca/recept9.log
        fi

        if  [ -e $4 ]; then
            exit 
        else 
    	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0660 -parameter $1$5$6        >> /home/orca/recept9.log
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0670 -parameter $1$5$6        >> /home/orca/recept9.log
        fi

        if  [ -e $4 ]; then
           exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0680 -parameter $1$5$6        >> /home/orca/recept9.log
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0690 -parameter $1$5$6        >> /home/orca/recept9.log
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0700 -parameter $1$5$6        >> /home/orca/recept9.log
        fi
 
        if  [ -e $4 ]; then
            exit 
        else 
           if  [ $7 != 'NOPG' ]; then
               $DBSTUB -dir $LDDIRECTORY -bd recept $7 -parameter $1$5$6       >> /home/orca/recept9.log

           fi
        fi

 
        if  [ -e $4 ]; then
            exit 
        else 
            if  [ $2 -eq '1' ]; then
##
##              レセ一括作成 
                $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0710 -parameter $9${10}$1$2$3$8$5$6    >> /home/orca/recept9.log
                if  [ -e $4 ]; then
                    exit 
                else
                    if  [ ${11} -eq '40' ]; then
                        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCRCP401 -parameter $9${10}$1$2$3$8$5$6 >> /home/orca/recept9.log
                    fi
                fi
            else
##
##              レセ個別作成 
                $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0710 -parameter $9${10}$1$2$3$8$5$6    >> /home/orca/recept9.log
                if  [ -e $4 ]; then
                    exit 
                else
                    if  [ ${14} -eq '40' ]; then
                        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCRCP401 -parameter $9${10}$1$2$3$8$5$6 >> /home/orca/recept9.log
                    fi
                fi
            fi
        fi

##
##レセ電作成        
        if  [ -e $4 ]; then
            exit 
        else
            if  [ ${12} -eq '2' ] || [ ${12} -eq '3' ] || [ ${13} -eq '2' ] || [ ${13} -eq '3' ]; then
                if ( ( [ ${12} != '2' ] && [ ${12} != '3' ] ) && [ ${3} -eq '1' ] ) || \
                   ( ( [ ${13} != '2' ] && [ ${13} != '3' ] ) && [ ${3} -eq '2' ] ); then
                    echo "no1"
                else
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0250 -parameter $1,$3,${12},${13},$5,$6 >> /home/orca/recept9.log
                    if  [ -e $4 ]; then
                        exit 
                    fi
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0260 -parameter $1,$3,${12},${13},$5,$6 >> /home/orca/recept9.log
                    if  [ -e $4 ]; then
                        exit 
                    fi
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0350 -parameter $1,$3,${12},${13},$5,$6 >> /home/orca/recept9.log
                fi 
            fi
        fi

        
        if  [ -e $4 ]; then
            exit 
        else
	    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCBJOB -parameter JBE$5$6 >> /home/orca/recept9.log
	fi
	
        exit 
