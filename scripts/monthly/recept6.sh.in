#!/bin/bash

if test -z "$JMARECEIPT_ENV" ; then
    JMARECEIPT_ENV="@jma-receipt-env@"
fi
if ! test -f "$JMARECEIPT_ENV"; then
    exit 0
fi

. $JMARECEIPT_ENV
#-------------------------------------------#
#    労災請求書用ファイル作成
#        CPCOMMONSHELL1.INC  
#        $1 SRYYM TERID SYSYMD
#        $2 SYOKBN
#           1:一括作成  2:個別作成
#        $3 RECEKBN
#           3:全体  
#        $4 エラーファイル名 
#        $5 JOBID 
#        $6 SHELLID
#        $8 処理年月
#        $9  作成年月日
#        $10 作成時間
#        $12 レセ電出力区分
#        $13 指定医療機関
#        $14 コメント記載記載区分
#        $15 枠記載区分
#        $16 HOSPNUM 
#        $17 様式区分
#        $18 レセプト作成区分
#            1:提出用レセ、2:点検用レセ、3:点検用レセ（０点公費記載）
#            4:点検用レセ（包括算定分記載）、5:点検用レセ（０点公費・包括算定分記載）
#        $19 請求データ削除区分
#            0:削除しない、2:削除する
#-------------------------------------------#

        if [ ${2} -eq  '1' ]; then
           LOG_FILE="/var/log/jma-receipt/${16}recept6a.log"
        else
           LOG_FILE="/var/log/jma-receipt/${16}recept6b.log"
        fi


        echo "arg1  = [" ${1}  "]"  >$LOG_FILE
        echo "arg2  = [" ${2}  "]" >>$LOG_FILE
        echo "arg3  = [" ${3}  "]" >>$LOG_FILE
        echo "arg4  = [" ${4}  "]" >>$LOG_FILE
        echo "arg8  = [" ${8}  "]" >>$LOG_FILE
        echo "arg9  = [" ${9}  "]" >>$LOG_FILE
        echo "arg10 = [" ${10} "]" >>$LOG_FILE
        echo "arg12 = [" ${12} "]" >>$LOG_FILE
        echo "arg13 = [" ${13} "]" >>$LOG_FILE
        echo "arg14 = [" ${14} "]" >>$LOG_FILE
        echo "arg15 = [" ${15} "]" >>$LOG_FILE
        echo "arg16 = [" ${16} "]" >>$LOG_FILE
        echo "arg17 = [" ${17} "]" >>$LOG_FILE
        echo "arg18 = [" ${18} "]" >>$LOG_FILE
        echo "arg19 = [" ${19} "]" >>$LOG_FILE

##      エラーファイル削除
	echo "errfile = [" ${MCP_TEMPDIR}/${4}  "]" >> $LOG_FILE
        rm  -f ${MCP_TEMPDIR}/${4}
        
        rm  -f  ${MCP_TEMPDIR}/${16}00RECE4*
        rm  -f  ${MCP_TEMPDIR}/${16}00RECEYMD*
        
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0400 -parameter $1$2$3$5$6,${17},${18},${16},${4}    >> $LOG_FILE 2>&1
        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit  
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0410 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi 

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0420 -parameter $1$3$5$6,${17},${18},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0425 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0428 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0430 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0435 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

 
        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0440 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
           exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0450 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi
 
        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0460 -parameter $1$3$5$6,${17},${18},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0465 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0466 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
	$DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0470 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1

        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
        $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0468 -parameter $1$3$5$6,${17},${16},${4}      >> $LOG_FILE 2>&1
        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit
        else 
           if [ ${17} -eq  '0' ]; then
              $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0480 -parameter $9${10}$1$2$3$8$5$6,${14},${15},${18},${19},${16},${4}    >> $LOG_FILE 2>&1
        
              if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                  exit 
              else 
                 $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0481 -parameter $9${10}$1$2$3$8$5$6,${14},${15},${18},${16},${4}    >> $LOG_FILE 2>&1
              fi
            

              if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                  exit 
              else 
                 $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0482 -parameter $9${10}$1$2$3$8$5$6,${14},${15},${18},${16},${4}    >> $LOG_FILE 2>&1
              fi
           else
              $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0501 -parameter $9${10}$1$2$3$8$5$6,${14},${18},${19},${16},${4}    >> $LOG_FILE 2>&1
        
              if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                  exit 
              else 
                 $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0502 -parameter $9${10}$1$2$3$8$5$6,${14},${18},${16},${4}    >> $LOG_FILE 2>&1
              fi
            

              if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                  exit 
              else 
                 $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0503 -parameter $9${10}$1$2$3$8$5$6,${14},${18},${16},${4}    >> $LOG_FILE 2>&1
              fi
	   fi
        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else 
           $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0483 -parameter $9${10}$1$2$3$8$5$6,${18},${16},${4}    >> $LOG_FILE 2>&1
        fi
##
##      レセ電作成        
        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else
            if  [ ${12} -eq '1' ] || [ ${12} -eq '3' ] ; then
                if  [ ${13} -eq '0' ]; then
                    echo "no receden " ${13}  >> $LOG_FILE 2>&1
                else
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0550 -parameter $1,$5,$6,${16},${4} >> $LOG_FILE 2>&1
                    if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                        exit 
                    fi
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0551 -parameter $1$3$5$6,${17},${16},${4}  >> $LOG_FILE 2>&1
                    if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                        exit 
                    fi
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0552 -parameter $1,$5,$6,${16},${4} >> $LOG_FILE 2>&1
                    if  [ -e ${MCP_TEMPDIR}/${4} ]; then
                        exit 
                    fi
                    $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCR0555 -parameter $1,$5,$6,${16},${9},${10},${18},${4} >> $LOG_FILE 2>&1
                fi
            fi
        fi

        if  [ -e ${MCP_TEMPDIR}/${4} ]; then
            exit 
        else
            $DBSTUB -dir $LDDIRECTORY -bd orcabt ORCBJOB -parameter JBE$5$6,${16}  >> $LOG_FILE 2>&1

            rm  -f  ${MCP_TEMPDIR}/${16}00RECE4*
            rm  -f  ${MCP_TEMPDIR}/${16}00RECEYMD*
	fi
	
        exit 
