              END-IF
              IF   SRY-SRYCD(IDX2)    =     CONS-SHUJUTU-COMMENT
                   MOVE   "1"   TO    RECE120-FLG-SHUJUTU-COMMENT
              END-IF
           END-IF
      *
           IF   RECE100-SRYYM     >=    "201004"
