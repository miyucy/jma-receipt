                                              CONS-ZAISIEN2         OR
                                              CONS-ZAISIEN3
                     MOVE   "1"   TO    RECE100-COMCNTFLG
                     MOVE   "1"   TO    RECE120-FLG-ZAISIEN-COMMENT
                  END-IF
                END-IF
                IF   RECE100-SRYYM   >=     "200804"
                  IF SRY-SRYCD(IDX2)    =     CONS-ZAISIEN1         OR
                                              CONS-ZAISIEN2         OR
                                              CONS-ZAISIEN3
                     MOVE   "1"   TO    RECE100-COMCNTFLG
                     MOVE   "1"   TO    RECE120-FLG-ZAISIEN-COMMENT
                  END-IF
                END-IF
              ELSE
                IF   WRK-SISETU695      =      1
                  IF   RECE100-SRYYM    >=     "200804"
                    IF SRY-SRYCD(IDX2)    =     CONS-ZAISIEN1       OR
                                                CONS-ZAISIEN2       OR
                                                CONS-ZAISIEN3
