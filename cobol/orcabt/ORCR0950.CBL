      *
      *****************************************************************
      *    医療機関情報検索
      *****************************************************************
       120-HOSPINF-GET-SEC            SECTION.
      *
           IF   WRK-PARA-SRYYM    NOT =  ZERO
      *       医療機関情報
              INITIALIZE                   SYSKANRI-REC
              MOVE    "1001"           TO  SYS-KANRICD
              MOVE    "*"              TO  SYS-KBNCD
              MOVE    WRK-PARA-SRYYM   TO  ORC-DBYMD (1:6)
              MOVE    "01"             TO  ORC-DBYMD (7:2) 
              PERFORM 960-KANRIMST-READ-SEC
              IF      FLG-SYSKANRI     =   ZERO
                  MOVE    MCPDATA-REC   TO  SYS-1001-REC
              ELSE    
                  MOVE    "医療機関情報が取得できませんでした。"
                                        TO  WRK-RECEERR
                  PERFORM 500-ERR-HENSYU-SEC
                  MOVE    2             TO  SPA-STS
                  GO  TO  120-HOSPINF-GET-EXT
              END-IF    
           ELSE
              MOVE     ZERO               TO  SYSKAN-IDX
              MOVE     SPACE              TO  TBL-SYS1001-T
              INITIALIZE                      TBL-SYS1001-T
      *
              INITIALIZE                      SYSKANRI-REC
              MOVE    "1001"              TO  SYS-KANRICD
              MOVE    "*"                 TO  SYS-KBNCD
              MOVE    SYSKANRI-REC        TO  MCPDATA-REC
              MOVE    "DBSELECT"          TO  MCP-FUNC
              MOVE    "SYSKANRI-KEY8"     TO  ORC-DBPATH
              CALL    "ORCMCPSUB"             USING
                                              MCPAREA
                                              ORCMCPAREA
                                              MCPDATA-REC
              IF      MCP-RC              =   ZERO
                  MOVE    "SYSKANRI-KEY8" TO  ORC-DBPATH
                  PERFORM  900-KANRIMST-NEXT-SEC
                  PERFORM  UNTIL   FLG-SYSKANRI  =    1
      *
                    MOVE    MCPDATA-REC   TO  SYS-1001-REC
                    ADD     1             TO  SYSKAN-IDX
                    MOVE    SYS-1001-REC  TO  TBL-SYS1001(SYSKAN-IDX)
      *
                    MOVE    "SYSKANRI-KEY8" TO  ORC-DBPATH
                    PERFORM 900-KANRIMST-NEXT-SEC
      *
                  END-PERFORM
              END-IF
              MOVE    "DBCLOSE"           TO  MCP-FUNC
              MOVE    "SYSKANRI-KEY8"     TO  ORC-DBPATH
              CALL    "ORCMCPSUB"          USING
                                           MCPAREA
                                           ORCMCPAREA
                                           MCPDATA-REC
              IF      SYSKAN-IDX          =   ZERO
                  MOVE    "医療機関情報が取得できませんでした。"
                                       TO  WRK-RECEERR
                  PERFORM 500-ERR-HENSYU-SEC
                  MOVE    2            TO  SPA-STS
                  GO  TO  120-HOSPINF-GET-EXT
              END-IF
           END-IF
      *
           IF   WRK-PARA-SRYYM    NOT =  ZERO
      *       医療機関情報（所在地ほか）
              INITIALIZE                   SYSKANRI-REC
              MOVE    "1002"           TO  SYS-KANRICD
              MOVE    "*"              TO  SYS-KBNCD
              MOVE    WRK-PARA-SRYYM   TO  ORC-DBYMD (1:6)
              MOVE    "01"             TO  ORC-DBYMD (7:2) 
              PERFORM 960-KANRIMST-READ-SEC
              IF      FLG-SYSKANRI     =   ZERO
                  MOVE    MCPDATA-REC  TO  SYS-1002-REC
              ELSE
              MOVE  "医療機関情報（所在地）が取得できませんでした。"
                                       TO  WRK-RECEERR
                  PERFORM 500-ERR-HENSYU-SEC
                  MOVE    2            TO  SPA-STS
                  GO  TO  120-HOSPINF-GET-EXT
              END-IF
           ELSE
              MOVE     ZERO               TO  SYSKAN-IDX
              MOVE     SPACE              TO  TBL-SYS1002-T
              INITIALIZE                      TBL-SYS1002-T
      *
              INITIALIZE                      SYSKANRI-REC
              MOVE    "1002"              TO  SYS-KANRICD
              MOVE    "*"                 TO  SYS-KBNCD
              MOVE    SYSKANRI-REC        TO  MCPDATA-REC
              MOVE    "DBSELECT"          TO  MCP-FUNC
              MOVE    "SYSKANRI-KEY8"     TO  ORC-DBPATH
              CALL    "ORCMCPSUB"             USING
                                              MCPAREA
                                              ORCMCPAREA
                                              MCPDATA-REC
              IF      MCP-RC              =   ZERO
                  MOVE    "SYSKANRI-KEY8" TO  ORC-DBPATH
                  PERFORM  900-KANRIMST-NEXT-SEC
                  PERFORM  UNTIL   FLG-SYSKANRI  =    1
      *
                    MOVE    MCPDATA-REC   TO  SYS-1002-REC
                    ADD     1             TO  SYSKAN-IDX
                    MOVE    SYS-1002-REC  TO  TBL-SYS1002(SYSKAN-IDX)
      *
                    MOVE    "SYSKANRI-KEY8" TO  ORC-DBPATH
                    PERFORM 900-KANRIMST-NEXT-SEC
      *
                  END-PERFORM
              END-IF
              MOVE    "DBCLOSE"           TO  MCP-FUNC
              MOVE    "SYSKANRI-KEY8"     TO  ORC-DBPATH
              CALL    "ORCMCPSUB"          USING
                                           MCPAREA
                                           ORCMCPAREA
                                           MCPDATA-REC
              IF      SYSKAN-IDX          =   ZERO
                  MOVE "医療機関情報（所在地）が取得できませんでした。"
                                       TO  WRK-RECEERR
                  PERFORM 500-ERR-HENSYU-SEC
                  MOVE    2            TO  SPA-STS
                  GO  TO  120-HOSPINF-GET-EXT
              END-IF
           END-IF
      *
           IF   WRK-PARA-SRYYM    NOT =  ZERO
      *       労災医療機関情報
              INITIALIZE                   SYSKANRI-REC
              MOVE    "4001"           TO  SYS-KANRICD
              MOVE    "*"              TO  SYS-KBNCD
              MOVE    WRK-PARA-SRYYM   TO  ORC-DBYMD (1:6)
              MOVE    "01"             TO  ORC-DBYMD (7:2) 
              PERFORM 960-KANRIMST-READ-SEC
              IF      FLG-SYSKANRI     =   ZERO
                  MOVE    MCPDATA-REC      TO  SYS-4001-REC
                  IF      SYS-4001-RSIIRYOCDN  =   LOW-VALUE
                      MOVE    SPACE        TO  SYS-4001-RSIIRYOCDN
                  END-IF     
              ELSE    
              MOVE    "労災自賠責医療機関情報が取得できませんでした。"
                                           TO  WRK-RECEERR
                  PERFORM 500-ERR-HENSYU-SEC
                  MOVE    2                TO  SPA-STS
                  GO  TO  120-HOSPINF-GET-EXT
              END-IF
           ELSE
              MOVE     ZERO               TO  SYSKAN-IDX
              MOVE     SPACE              TO  TBL-SYS4001-T
              INITIALIZE                      TBL-SYS4001-T
      *
              INITIALIZE                      SYSKANRI-REC
              MOVE    "4001"              TO  SYS-KANRICD
              MOVE    "*"                 TO  SYS-KBNCD
              MOVE    SYSKANRI-REC        TO  MCPDATA-REC
              MOVE    "DBSELECT"          TO  MCP-FUNC
              MOVE    "SYSKANRI-KEY8"     TO  ORC-DBPATH
              CALL    "ORCMCPSUB"             USING
                                              MCPAREA
                                              ORCMCPAREA
                                              MCPDATA-REC
              IF      MCP-RC              =   ZERO
                  MOVE    "SYSKANRI-KEY8" TO  ORC-DBPATH
                  PERFORM 900-KANRIMST-NEXT-SEC
                  PERFORM  UNTIL   FLG-SYSKANRI  =    1
      *
                    MOVE    MCPDATA-REC   TO  SYS-4001-REC
                    ADD     1             TO  SYSKAN-IDX
                    MOVE    SYS-4001-REC  TO  TBL-SYS4001(SYSKAN-IDX)
      *
                    MOVE    "SYSKANRI-KEY8" TO  ORC-DBPATH
                    PERFORM 900-KANRIMST-NEXT-SEC
      *
                  END-PERFORM
              END-IF
              MOVE    "DBCLOSE"           TO  MCP-FUNC
              MOVE    "SYSKANRI-KEY8"     TO  ORC-DBPATH
              CALL    "ORCMCPSUB"          USING
                                           MCPAREA
                                           ORCMCPAREA
                                           MCPDATA-REC
              IF      SYSKAN-IDX          =   ZERO
              MOVE    "労災自賠責医療機関情報が取得できませんでした。"
                                           TO  WRK-RECEERR
                  PERFORM 500-ERR-HENSYU-SEC
                  MOVE    2               TO  SPA-STS
                  GO  TO  120-HOSPINF-GET-EXT
              END-IF
           END-IF
      *
           .
       120-HOSPINF-GET-EXT.
           EXIT.
      *
      *****************************************************************
      *    主　　処理
      *****************************************************************
       200-MAIN-SEC                SECTION.
      *
           PERFORM         UNTIL   FLG-RECE82     =   1
                           OR      SPA-STS        =   2 
      *
      *        療養期間編集処理
               PERFORM 2001-SRYYM-HENSYU-SEC
               MOVE    KEY-NEW         TO  KEY-OLD
               PERFORM         UNTIL   FLG-RECE82      =   1
                               OR      SPA-STS         =   2
                               OR      KEY-N-SRYYM NOT =   KEY-O-SRYYM
      *            レセプト編集処理
                   MOVE    ZERO            TO  CNT-KENSU
                   MOVE    KEY-NEW         TO  KEY-OLD
                   PERFORM         UNTIL   FLG-RECE82      =   1
                                   OR      SPA-STS         =   2
                                   OR      KEY-NEW     NOT =   KEY-OLD
                       ADD     1                       TO  CNT-KENSU
                       PERFORM 2002-RECEIPT-HENSYU-SEC
      *
                       PERFORM 990-RECE82-READ-SEC
                   END-PERFORM
               END-PERFORM
           END-PERFORM    
      *
           MOVE    2               TO  SPA-STS
           .
       200-MAIN-EXT.
           EXIT.
      *
      *****************************************************************
      *    療養期間編集処理      
      *****************************************************************
        2001-SRYYM-HENSYU-SEC      SECTION.
      *     
      *    療養期間開始編集
           MOVE    RECE82-SRYYM    TO  WRK-STYMD (1:6)
           MOVE    "01"            TO  WRK-STYMD (7:2)
           MOVE    WRK-STYMD       TO  WRK-SYMD
           PERFORM 31012-SE-WA-YMDEDIT-SEC
           MOVE    WRK-HENYMD      TO  WRK-STYMDW
      *     
      *    療養期間終了編集
           INITIALIZE                  STS-AREA-DAY
           INITIALIZE                  LNK-DAY7-AREA
           MOVE    "71"            TO  LNK-DAY7-IRAI
           MOVE    RECE82-SRYYM    TO  LNK-DAY7-YM
           CALL    "ORCSDAY"           USING   STS-AREA-DAY
                                               LNK-DAY7-AREA
           MOVE    LNK-DAY7-KOYOMI TO  WRK-EDYMD 
           MOVE    WRK-EDYMD       TO  WRK-SYMD
           PERFORM 31012-SE-WA-YMDEDIT-SEC
           MOVE    WRK-HENYMD      TO  WRK-EDYMDW
           . 
        2001-SRYYM-HENSYU-EXT.
           EXIT.
      *
      *****************************************************************
      *    レセプト編集処理
      *****************************************************************
       2002-RECEIPT-HENSYU-SEC                SECTION.
      *
           MOVE    1         TO  CNT-PAGE
           INITIALIZE                  WRK-EDIT-AREA
      *
      *    帳票編集<ヘッダー部>処理
           PERFORM 20021-HEAD-HEN-SEC
      *
      *    帳票編集<点数>処理
           PERFORM 20022-TENSU-HEN-SEC
      *
      *    帳票編集<傷病名>処理
           PERFORM 20023-BYOMEI-HEN-SEC
      *
      *    帳票編集<傷病の経過>処理
           PERFORM 20024-COMMENT-HEN-SEC 
      *
      *    帳票編集<入退院履歴・入院基本料種別の続き>処理
           PERFORM 380-HC101-NYUTAI-RIREKI-HEN-SEC
      *
      *    帳票編集<手術歴>処理
           PERFORM 380-HC101-OPECOM-HEN-SEC 
      *
      *    帳票編集<適用（点数）>処理
           PERFORM 20025-ZAI-HEN-SEC
      *
      *    帳票編集<適用（金額）>処理
           PERFORM 20026-ZAI-HEN1-SEC
      *
      *    帳票印刷処理（続紙）
           IF      CNT-PAGE           >   1
               PERFORM 20027-PRINT-OUT-SEC
           END-IF    
      *
      *    帳票印刷処理
           IF      SPA-STS            =   ZERO
               MOVE    1                  TO  CNT-PAGE
               PERFORM 20027-PRINT-OUT-SEC
           END-IF    
      *
           .
       2002-RECEIPT-HENSYU-EXT.
           EXIT.
      *     
      *****************************************************************
      *    帳票編集<ヘッダー部>処理
      *****************************************************************
       20021-HEAD-HEN-SEC          SECTION.
      *
           MOVE    WRK-TEKIYOU-LINE    TO  WRK-LINE-MAX
      *     
           INITIALIZE                      HC22
      *
      *    災害区分
           IF      RECE82-RECESYUBETU  =   ZERO
               CONTINUE
           ELSE    
               EVALUATE    RECE82-SAIGAIKBN
                   WHEN    "1"
                       MOVE    "ギョウム"      TO  HC22-SAIGAIKBN
                   WHEN    "2"
                       MOVE    "ツウサイ"      TO  HC22-SAIGAIKBN
               END-EVALUATE
           END-IF        
      *    患者番号
           MOVE    RECE82-PTNUM    TO  HC22-PTNUM
      *    指定病院等の番号
           MOVE    SYS-4001-RSIIRYOCDN TO  HC22-RSIIRYOCD
      *    病院等の名称
           MOVE    SYS-1001-HOSPNAME   TO  HC22-HOSPNAME
      *    労働保険者番号
           MOVE    RECE82-KOFUNUM      TO  HC22-KOFUNUM
      *    新継区分
           MOVE    RECE82-SINKEI       TO  HC22-SINKEI
      *    転帰事由
           MOVE    RECE82-TENKI        TO  HC22-TENKI
      *    生年月日
           MOVE    RECE82-BIRTHDAY     TO  WRK-SYMD
           PERFORM 31012-SE-WA-YMDEDIT-SEC
           EVALUATE    WRK-HENGENGO
               WHEN    "M"
                   MOVE    "1"             TO  HC22-TANJYOYMD (1:1)      
               WHEN    "T"
                   MOVE    "3"             TO  HC22-TANJYOYMD (1:1)      
               WHEN    "S"
                   MOVE    "5"             TO  HC22-TANJYOYMD (1:1)      
               WHEN    "H"
                   MOVE    "7"             TO  HC22-TANJYOYMD (1:1)      
           END-EVALUATE     
           MOVE    WRK-HENYMD          TO  HC22-TANJYOYMD (2:)
      *    傷病年月日
           MOVE    RECE82-SHOBYOYMD    TO  WRK-SYMD
           PERFORM 31012-SE-WA-YMDEDIT-SEC
           MOVE    WRK-HENYMD          TO  HC22-SYOBYOYMD 
      *    療養開始年月日
           IF    RECE82-RYOSTYMD   NOT =  SPACE
             IF      RECE82-RYOSTYMD     <   WRK-STYMD
                 MOVE    WRK-STYMDW          TO  HC22-STRYMD
             ELSE      
                 MOVE    RECE82-RYOSTYMD     TO  WRK-SYMD
                 PERFORM 31012-SE-WA-YMDEDIT-SEC
                 MOVE    WRK-HENYMD          TO  HC22-STRYMD
             END-IF    
           ELSE
             MOVE   WRK-STYMDW(1:4)   TO   HC22-STRYMD (1:4)
             PERFORM VARYING IDX FROM    1   BY  1
                     UNTIL   IDX >       31
                 IF      RECE82-DAY (IDX)    =   RECE82-HKNCOMBI
                     MOVE    IDX                 TO  WRK-KGK2
                     MOVE    WRK-KGK2            TO  HC22-STRYMD (5:2)
                     MOVE    31                  TO  IDX
                 END-IF
             END-PERFORM
           END-IF
      *    療養終了年月日
           IF    RECE82-RYOEDYMD   NOT =  SPACE
             IF      RECE82-RYOEDYMD     >   WRK-EDYMD
                 MOVE    WRK-EDYMDW          TO  HC22-ENDYMD
             ELSE      
                 MOVE    RECE82-RYOEDYMD     TO  WRK-SYMD
                 PERFORM 31012-SE-WA-YMDEDIT-SEC
                 MOVE    WRK-HENYMD          TO  HC22-ENDYMD
             END-IF    
           ELSE
             MOVE   WRK-EDYMDW(1:4)   TO   HC22-ENDYMD (1:4)
             PERFORM VARYING IDX FROM    31  BY  -1
                     UNTIL   IDX <       1
                 IF      RECE82-DAY (IDX)    =   RECE82-HKNCOMBI
                     MOVE    IDX                 TO  WRK-KGK2
                     MOVE    WRK-KGK2            TO  HC22-ENDYMD (5:2)
                     MOVE    1                   TO  IDX
                 END-IF
             END-PERFORM
           END-IF
      *    実日数
           MOVE    RECE82-JNISSU       TO  WRK-JNISSU
           MOVE    WRK-JNISSU          TO  HC22-NISSU
      *    合計額
           COMPUTE WRK-GOKEI   =   RECE82-TOTALKIN
                               +   RECE82-KINGAKU2-SYO
                               +   RECE82-SHOKUJITOTALKIN
           MOVE    WRK-GOKEI           TO  WRK-KHNTENZ
           MOVE    WRK-KHNTENZ         TO  HC22-GOKEI
      *    カナ氏名
           MOVE    RECE82-KANANAME     TO  HC22-KANANAME
      *    氏名
           MOVE    ZERO    TO   IDW2
           PERFORM   VARYING   IDW1   FROM   50   BY  -2
                           UNTIL   IDW1   <   1
               IF   RECE82-NAME(IDW1 - 1:2)  NOT =   SPACE
                           COMPUTE IDW2 = IDW1 / 2
                           MOVE   2   TO  IDW1
               END-IF
           END-PERFORM
           IF     IDW2   > 13
                MOVE    RECE82-NAME     TO  HC22-NAME2
           ELSE
                MOVE    RECE82-NAME     TO  HC22-NAME
           END-IF
      *    年齢
           MOVE    RECE82-BIRTHDAY     TO  WRK-BIRTHDAY    
           COMPUTE WRK-NENREI          =   RECE82-SRYYM
                                       -   WRK-BIRTHYM
           MOVE    WRK-NENREI-R1       TO  WRK-NENREI-X                            
           MOVE    WRK-NENREI-X        TO  HC22-NENREI 
      *    事業の名称
           MOVE    RECE82-JIGYOUNAME   TO  HC22-JIGYOUNAME
      *    事業場の所在地
           MOVE    RECE82-PREFNAME     TO  HC22-TODOFUKEN
           MOVE    RECE82-CITYNAME     TO  HC22-GUNSIKU
           EVALUATE    RECE82-PREFKBN
               WHEN    "1"  
                   MOVE    "○"        TO  HC22-TODOFUKENKBN1
               WHEN    "2"  
                   MOVE    "○"        TO  HC22-TODOFUKENKBN2
               WHEN    "3"  
                   MOVE    "○"        TO  HC22-TODOFUKENKBN3
               WHEN    "4"  
                   MOVE    "○"        TO  HC22-TODOFUKENKBN4
           END-EVALUATE
           EVALUATE    RECE82-CITYKBN
               WHEN    "1"
                   MOVE    "○"        TO  HC22-GUNSIKUKBN1
               WHEN    "2"
                   MOVE    "○"        TO  HC22-GUNSIKUKBN2
               WHEN    "3"
                   MOVE    "○"        TO  HC22-GUNSIKUKBN3
           END-EVALUATE                             
      *
           .
       20021-HEAD-HEN-EXT.
           EXIT.
      *     
      *****************************************************************
      *    帳票編集<続紙ヘッダー部>処理
      *****************************************************************
       310-HC24-HEAD-HEN-SEC          SECTION.
      *
           MOVE    WRK-TEKIYOU-HC24-LINE    TO  WRK-LINE-MAX
      *
           INITIALIZE                 HC24
      *      
      *    災害区分
           EVALUATE    RECE82-SAIGAIKBN
               WHEN    "1"
                   MOVE    "ギョウム"      TO  HC24-SAIGAIKBN
               WHEN    "2"
                   MOVE    "ツウサイ"      TO  HC24-SAIGAIKBN
           END-EVALUATE        
      *    患者番号
           STRING   RECE82-PTNUM   DELIMITED   BY  SPACE
                    "-"            DELIMITED   BY  SIZE
                    CNT-PAGE (3:1) DELIMITED   BY  SIZE
                                   INTO    HC24-PTNUM
           END-STRING
      *    指定病院等の番号
           MOVE    SYS-4001-RSIIRYOCDN TO  HC24-HOSPNUM
      *    病院等の名称
           MOVE    SYS-1001-HOSPNAME   TO  HC24-HOSPNAME
      *
           EVALUATE    RECE82-HKNKBN
               WHEN    "1"
      *            労働保険者番号
                   MOVE    RECE82-KOFUNUM      TO  HC24-HKNNUM 
               WHEN    "2"
      *            年金証書番号
                   MOVE    RECE82-KOFUNUM      TO  HC24-NENNUM
           END-EVALUATE        
      *    氏名
           MOVE    RECE82-NAME         TO  HC24-PTNAME
      *    カナ氏名
           MOVE    RECE82-KANANAME     TO  HC24-PTKANANAME
      *    年齢
           MOVE    WRK-NENREI-X        TO  HC24-PTAGE
      *
           .
       310-HC24-HEAD-HEN-EXT.
           EXIT.
      *
      *****************************************************************
      *    帳票編集<傷病名>処理
      *****************************************************************
       20023-BYOMEI-HEN-SEC      SECTION.
      *     
           MOVE    SPACE               TO  WRK-BYOMEI1
                                           WRK-BYOMEI2
      *
      *    傷病の部位編集
           IF      RECE82-COMMENT      =   SPACE
               CONTINUE
           ELSE
               ADD     1               TO  WRK-LINEBYO
               MOVE    RECE82-COMMENT  TO  HC22-SYOBYOMEI (WRK-LINEBYO)
               IF      RECE82-COMMENT (45:)    NOT =   SPACE
                   ADD     1           TO  WRK-LINEBYO
                   MOVE    RECE82-COMMENT (45:44)
                                       TO  HC22-SYOBYOMEI (WRK-LINEBYO)
               END-IF                        
               IF      RECE82-COMMENT (89:)    NOT =   SPACE
                   ADD     1           TO  WRK-LINEBYO
                   MOVE    RECE82-COMMENT (89:)
                                       TO  HC22-SYOBYOMEI (WRK-LINEBYO)
              END-IF                        
           END-IF
      *
      *    傷病名の編集                    
           INITIALIZE                      RECE83-REC
           MOVE    RECE82-HOSPID       TO  RECE83-HOSPID
           MOVE    RECE82-SRYYM        TO  RECE83-SRYYM
           MOVE    RECE82-NYUGAIKBN    TO  RECE83-NYUGAIKBN
           MOVE    RECE82-PTID         TO  RECE83-PTID
           MOVE    RECE82-RECEKA       TO  RECE83-RECEKA
           MOVE    RECE82-HKNID        TO  RECE83-HKNID
           MOVE    RECE82-TEKSTYMD     TO  RECE83-TEKSTYMD
      *
           START   RECE83-FILE
               KEY IS    >=   RECE83-KEY
               INVALID
                   MOVE    1           TO  FLG-RECE83
               NOT INVALID
                   PERFORM 990-RECE83-NEXT-SEC
           END-START
      *
           PERFORM UNTIL ( FLG-RECE83           =   1                )
                   OR    ( RECE82-HOSPID    NOT =   RECE83-HOSPID    )
                   OR    ( RECE82-SRYYM     NOT =   RECE83-SRYYM     )
                   OR    ( RECE82-NYUGAIKBN NOT =   RECE83-NYUGAIKBN )
                   OR    ( RECE82-PTID      NOT =   RECE83-PTID      )
                   OR    ( RECE82-RECEKA    NOT =   RECE83-RECEKA    )
                   OR    ( RECE82-HKNID     NOT =   RECE83-HKNID     )
                   OR    ( RECE82-TEKSTYMD  NOT =   RECE83-TEKSTYMD  )
      *        病名編集処理
               PERFORM 200231-BYOMEI-SET-SEC
           END-PERFORM
      *
           IF      WRK-LINEBYO         <  WRK-BYOMEI-LINE
               IF      WRK-BYOMEI1-NAME   NOT =   SPACE
                   PERFORM 200232-BYOMEI1-OUT-SEC
               END-IF
           ELSE
               IF      WRK-BYOMEI2-NAME   NOT =   SPACE
                   PERFORM 200233-BYOMEI2-OUT-SEC
               END-IF
           END-IF
           .
       20023-BYOMEI-HEN-EXT.
