                               WHEN   "1"
                                  MOVE  "5"     TO   RECE62-SHOSHINTIME
                               WHEN   "2"
                                  MOVE  "6"     TO   RECE62-SHOSHINTIME
                               WHEN   "3"
                                  CONTINUE
                               WHEN   "4"
