      *
           IF  ( FLG-CHIIKI        NOT = ZERO )
           OR  ((FLG-HOKENICHIBU2  NOT = ZERO) AND
                (FLG-HOKENICHIBU3  NOT = ZERO) AND
                (FLG-HOKENICHIBU-SYU   = ZERO) AND
                (FLG-CHIIKI            = ZERO))
           OR  ( FLG-HOKENICHIBU-CHK   =  1   )
             IF   WRK-KINGAKU3  >  WRK-CHK-KOKIGAK2
