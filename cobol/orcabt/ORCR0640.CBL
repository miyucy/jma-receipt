           IF      NACCT-ZAISKBKBN      =       "1"
             IF  (NACCT-SRYCDTOTAL =   190799410 )
             OR  (NACCT-SRYCDTOTAL =   190799510 )
                MOVE    "90"            TO      RECE65-SRYKBN
                MOVE    "900"           TO      RECE65-SRYSYUKBN
             END-IF
