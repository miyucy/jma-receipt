                       (TOKU75-IDX2                 =  1      ) AND
                       (RECE62-SRYYM               >= "200905"))
                   OR ((FLG-TOKU75  = 1 OR 2                  ) AND
                       (WRK-PREFNUM                 = "23"    ) AND
                       (COMB-KOHHKNNUM(TOKU75-IDX2) = "191"   ) AND
                       (TOKU75-IDX2                 =  1      ) AND
                       (RECE62-SRYYM               >= "200910"))
                   OR ((FLG-TOKU75  = 1 OR 2                  ) AND
                       (WRK-PREFNUM                 = "23"
                                                   OR "24"    ) AND
                       (COMB-KOHHKNNUM(TOKU75-IDX2) = "152"   ) AND
                       (TOKU75-IDX2                 =  1      ) AND
                       (RECE62-SRYYM               >= "201104"))
