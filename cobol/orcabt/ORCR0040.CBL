               OR     ( RECE049-HKNJANUM  NOT =   RECE059-HKNJANUM     )
               OR     ( RECE049-HOJOKBN   NOT =   RECE059-HOJOKBN      )
               OR     ( RECE049-TEKSTYMD  NOT =   RECE059-TEKSTYMD     )
