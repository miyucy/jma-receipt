      *****************************************************************
      *    主　　処理
      *****************************************************************
       000-PROC-SEC                SECTION.
      *
           PERFORM 100-INIT-SEC
      *
           PERFORM 200-MAIN-SEC
                   UNTIL        FLG-END    =   1
      *
           PERFORM 300-END-SEC
      *
           STOP    RUN
           .
      *****************************************************************
      *    初期　処理
      *****************************************************************
       100-INIT-SEC                SECTION.
      *
           INITIALIZE                  FLG-AREA
           INITIALIZE                  STS-AREA
           INITIALIZE                  SYS-AREA
           INITIALIZE                  WRK-AREA
           INITIALIZE                  IDX-AREA
           INITIALIZE                  SPA-AREA
           INITIALIZE                  TBL-SRYACT-AREA
           INITIALIZE                  TBL-SRYKACD-AREA
           MOVE    ZERO            TO  CNT-AREA
      *
      *    パラメタセット
           UNSTRING   COMMAND-PARAM    DELIMITED  BY  ","
                                       INTO    LNK-PRTKANRI-RENNUM
                                               LNK-PRTKANRI-TBL-KEY
                                               LNK-PRTKANRI-TBL-GROUP
                                               LNK-PRTKANRI-SHORI-RENNUM
                                               LNK-PRTKANRI-SRYYM
                                               LNK-PRTKANRI-SKYYMD
                                               LNK-PRTKANRI-SHELLID
                                               LNK-PRTKANRI-PRIORITY
                                               LNK-PRTKANRI-TERMID
                                               LNK-PRTKANRI-OPID
                                               LNK-PRTKANRI-PRTNM
                                               WRK-PARA-JOBID
                                               WRK-PARA-SHELLID
                                               WRK-PARA-HOSPNUM
                                               RECEERR
                                               WRK-PARA-BASE-YMD
                                               WRK-PARA-NYUGAIKBN
                                               WRK-PARA-MONBLOB-ID
                                               WRK-PARA-KARTE-UID
                                               WRK-PARA-ORCA-UID 
           END-UNSTRING
      *
           DISPLAY "NYUGAIKBN =" WRK-PARA-NYUGAIKBN
           DISPLAY "MONBLOB-ID=" WRK-PARA-MONBLOB-ID
      *
      *    ＳＰＡ設定
           MOVE    WRK-PARA-HOSPNUM    TO  SPA-HOSPNUM
      *
           INITIALIZE                      SGETTEMP-AREA
           MOVE    RECEERR             TO  SGETTEMP-BASENAMES  (1)
           CALL    "ORCSGETTEMP"       USING   SGETTEMP-AREA
           MOVE    SPACE               TO  RECEERR
           MOVE    SGETTEMP-FULLNAMES (1)
                                       TO  RECEERR
      *
           PERFORM 100-DBOPEN-SEC
      *
      *    ステップ管理開始処理
           MOVE    "STS"               TO  SJOBKANRI-MODE
           INITIALIZE                      JOBKANRI-REC
           MOVE    "ORCBSTC001"        TO  JOB-PGID
           MOVE    "在庫管理データ作成"
                                       TO  JOB-SHELLMSG
           PERFORM 900-CALL-ORCSJOB-SEC
      *
      *    初期値セット
           PERFORM 110-SYOKI-SET-SEC
      *
           .
       100-INIT-EXT.
           EXIT.
      *
      *****************************************************************
      *    初期値セット
      *****************************************************************
       110-SYOKI-SET-SEC           SECTION.
      *
      *    システム日付セット
           MOVE    LNK-PRTKANRI-SKYYMD
                                   TO  WRK-SYSYMD
           INITIALIZE                  STS-AREA-DAY
           INITIALIZE                  LNK-DAY1-AREA
           MOVE    "11"            TO  LNK-DAY1-IRAI
           MOVE    WRK-SYSYMD      TO  LNK-DAY1-YMD
           CALL    "ORCSDAY"           USING
                                       STS-AREA-DAY
                                       LNK-DAY1-AREA
           IF      STS-DAY-RC1         =   ZERO
               MOVE    WRK-SYSYMD          TO  SPA-SYSYMD
           END-IF
      *
      *    入外区分
           MOVE    WRK-PARA-NYUGAIKBN  TO  SPA-NYUGAIKBN
      *
           .
       110-SYOKI-SET-EXT.
           EXIT.
      *
      *****************************************************************
      *    主　　処理
      *****************************************************************
       200-MAIN-SEC                SECTION.
      *
      *    受診歴を検索する  医療機関、基準日以降
           INITIALIZE                  JYURRK-REC
           MOVE    SPA-HOSPNUM         TO  JYURRK-HOSPNUM
           MOVE    SPA-NYUGAIKBN       TO  JYURRK-NYUGAIKBN
           MOVE    WRK-PARA-BASE-YMD   TO  JYURRK-UPYMD
      *
           MOVE    "key66"             TO  WRK-MCP-PATHNAME
      *
           MOVE    JYURRK-REC          TO  MCPDATA-REC
           MOVE    "tbl_jyurrk"        TO  MCP-TABLE
           MOVE    WRK-MCP-PATHNAME    TO  MCP-PATHNAME
           PERFORM 910-DBSELECT-SEC
           IF      MCP-RC              =   ZERO
               MOVE    "tbl_jyurrk"        TO  MCP-TABLE
               MOVE    WRK-MCP-PATHNAME    TO  MCP-PATHNAME
               PERFORM 970-JYURRK-READ-SEC
           ELSE
               INITIALIZE                  JYURRK-REC
               MOVE    1               TO  FLG-JYURRK
           END-IF
      *
           MOVE    ZERO                TO  IDX
           MOVE    ZERO                TO  IDY
           MOVE    ZERO                TO  IDX-S
           MOVE    JYURRK-DENPNUM      TO  WRK-KEY-DENPNUM
           INITIALIZE                  TBL-SRYACT-AREA
           PERFORM
               UNTIL   (FLG-JYURRK         =   1    )
                  OR   (FLG-ERR        NOT =   ZERO)
               IF      JYURRK-DENPNUM      NOT =   WRK-KEY-DENPNUM
                   IF      IDX-S               >   ZERO
                       PERFORM 20022-STOCKRES-HEN-SEC
                   END-IF
                   INITIALIZE                  TBL-SRYACT-AREA
                   MOVE    ZERO                TO  IDX-S
               END-IF
               MOVE    JYURRK-DENPNUM      TO  WRK-KEY-DENPNUM
               MOVE    JYURRK-SRYYMD       TO  WRK-KEY-SRYYMD
               MOVE    JYURRK-SRYKA        TO  WRK-KEY-SRYKA
      *
               IF      FLG-ERR             =   ZERO
                   PERFORM 20021-JYURRK-SYORI-SEC
               END-IF
      *
               MOVE    "tbl_jyurrk"        TO  MCP-TABLE
               MOVE    WRK-MCP-PATHNAME    TO  MCP-PATHNAME
               PERFORM 970-JYURRK-READ-SEC
      *
           END-PERFORM
           MOVE    "tbl_jyurrk"        TO  MCP-TABLE
           MOVE    WRK-MCP-PATHNAME    TO  MCP-PATHNAME
           PERFORM 990-DBCLOSE-SEC
      *
           IF      IDX-S               >   ZERO
               PERFORM 20022-STOCKRES-HEN-SEC
           END-IF
      *
           MOVE    1                  TO  FLG-END
           .
       200-MAIN-EXT.
           EXIT.
      *****************************************************************
      *    剤内容編集処理
      *****************************************************************
       20021-JYURRK-SYORI-SEC          SECTION.
      *
           PERFORM VARYING     IDX-J   FROM    1   BY  1
                   UNTIL      (IDX-J       >   15      )
                           OR (FLG-ERR     NOT =   ZERO)
               IF      JYURRK-ZAINUM (IDX-J)   NOT =   ZERO
                   PERFORM 20034-ZAISET-HEN-SEC
               END-IF
           END-PERFORM
           .
       20021-JYURRK-SYORI-EXT.
           EXIT.
      *
      *****************************************************************
      *    剤内容編集処理
      *****************************************************************
       20034-ZAISET-HEN-SEC          SECTION.
      *
      *    診療会計
           INITIALIZE                      SRYACCT-REC
           MOVE    JYURRK-HOSPNUM      TO  ACCT-HOSPNUM
           MOVE    JYURRK-NYUGAIKBN    TO  ACCT-NYUGAIKBN
           MOVE    JYURRK-PTID         TO  ACCT-PTID
           MOVE    JYURRK-SRYKA        TO  ACCT-SRYKA
           MOVE    JYURRK-SRYYMD(1:6)  TO  ACCT-SRYYM
           MOVE    JYURRK-ZAINUM (IDX-J)
                                       TO  ACCT-ZAINUM
      *
           MOVE    SRYACCT-REC         TO  MCPDATA-REC
           MOVE    "tbl_sryacct"       TO  MCP-TABLE
           MOVE    "key5"              TO  MCP-PATHNAME
           PERFORM 910-DBSELECT-SEC
           IF      MCP-RC              =   ZERO
               MOVE    "tbl_sryacct"       TO  MCP-TABLE
               MOVE    "key5"              TO  MCP-PATHNAME
               PERFORM 930-SRYACCT-READ-SEC
           ELSE
               MOVE    1                   TO  FLG-SRYACCT
               INITIALIZE                      SRYACCT-REC
           END-IF
           MOVE    "tbl_sryacct"       TO  MCP-TABLE
           MOVE    "key5"              TO  MCP-PATHNAME
           PERFORM 990-DBCLOSE-SEC
      *
           MOVE    JYURRK-SRYYMD(7:2)  TO  IDX-ACT
      *    受診連番
           COMPUTE IDX-REN             =   JYURRK-RENNUM
                                       +   1
      *
           MOVE    ACCT-DAY (IDX-REN IDX-ACT)
                                       TO  WRK-KAISU
      *
           MOVE    ZERO                TO  FLG-CHK
      *    回数ゼロは対象外
           IF     (WRK-KAISU           =   ZERO )
               MOVE    1                   TO  FLG-CHK
           END-IF
      *
      *    薬剤がない時
           IF      (SRY-SRYKBN         =   "11"
                                       OR  "12"
                                       OR  "13"
                                       OR  "99"
                                       OR  "97"
                                       OR  "98" )
               MOVE    1                   TO  FLG-CHK
           END-IF
           IF     (FLG-CHK             =   1     )
               GO      TO      20034-ZAISET-HEN-EXT
           END-IF
      *    診療行為テーブル検索
           INITIALIZE                  SRYACT-REC
           MOVE    ACCT-HOSPNUM        TO  SRY-HOSPNUM
           MOVE    ACCT-NYUGAIKBN      TO  SRY-NYUGAIKBN
           MOVE    ACCT-PTID           TO  SRY-PTID
           MOVE    ACCT-SRYKA          TO  SRY-SRYKA
           MOVE    ACCT-SRYYM          TO  SRY-SRYYM
           MOVE    ACCT-ZAINUM         TO  SRY-ZAINUM
      *
           MOVE    SRYACT-REC          TO  MCPDATA-REC
           MOVE    "tbl_sryact"        TO  MCP-TABLE
           MOVE    "key2"              TO  MCP-PATHNAME
           PERFORM 910-DBSELECT-SEC
           IF      MCP-RC              =   ZERO
               MOVE    "tbl_sryact"        TO  MCP-TABLE
               MOVE    "key2"              TO  MCP-PATHNAME
               PERFORM 920-SRYACT-READ-SEC
           ELSE
               INITIALIZE                  SRYACT-REC
               MOVE    1               TO  FLG-SRYACT
           END-IF
      *    院外投薬は対象外
           IF      (SRY-SRYKBN         =   "21"
                                       OR  "22"
                                       OR  "23" )
              AND  (ACCT-ZAITEN        =   ZERO )
               IF      SRY-SRYSYUKBN       =   "213"
                                           OR  "223"
                                           OR  "233"
                   CONTINUE
               ELSE
                   MOVE    1               TO  FLG-CHK
               END-IF
           END-IF
      *    在宅（院外）
           IF      SRY-SRYSYUKBN       =   "148"
                                       OR  "149"
               MOVE    1               TO  FLG-CHK
           END-IF
      *
           PERFORM
               UNTIL  ( FLG-SRYACT      =   1  )
                  OR  ( FLG-CHK         =   1  )
               PERFORM VARYING     IDXA    FROM    1   BY  1
                       UNTIL      (IDXA    >   5   )   OR
                                  (SRY-SRYCD (IDXA)    =   SPACE)
      *            薬剤のみ
                   IF      (SRY-SRYCD(IDXA)(1:1)   =   "6" )
      *             投薬のユーザ登録
                    OR    ((SRY-SRYCD(IDXA)(1:3)   =   "059")
                       AND (SRY-SRYKBN(1:1)        =   "2"  ))
                       PERFORM 200341-MEISE-HEN-SEC
                   END-IF
               END-PERFORM
      *
               MOVE    "tbl_sryact"        TO  MCP-TABLE
               MOVE    "key2"              TO  MCP-PATHNAME
               PERFORM 920-SRYACT-READ-SEC
