                                                 <  RECE41-SRYYM )
                                MOVE   "5"      TO  RECE41-SINKEI
                            END-IF
                           WHEN    "2"  
                            IF   ( PTRSI-SINKEI = "1" OR "7"        )
                            AND  ( PTRSI-RYOSTYMD(1:6) 
