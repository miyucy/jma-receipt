                 END-IF
               ELSE
                 IF      FLG-ZENKOH-ARI  =  ZERO
                   IF  FLG-WRKFILE-CLEAR  =  ZERO
                   OR  FLG-WRKFILE-KGKTAI =   1
                     IF   FLG-ZENKOKUKOHI-NISYU  =  4
                       IF  RECE02-KOHNUM(1) = SYU-KOH-HKNNUM(1)
