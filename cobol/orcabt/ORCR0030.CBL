                     ( RECE02-KOHFTNJANUM(4)(5:2) = "80" OR "70"   ) AND
                     ( RECE02-KOHFTNJANUM(4)(1:2) = "52"           ) AND
                     ( RECE02-SRYYM              >= "201501"       ))
                 OR (( RECE02-KOHNUM(4)    = "054"                 ) AND
