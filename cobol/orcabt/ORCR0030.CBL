                                            OR "099209905"
                                            OR "099209906"
                                            OR "099209907"
                                            OR "099209908"  )
             OR     ( SRY-SRYCD(IDX2)(1:4)  =  "0085"
                                            OR "0086"       )
             OR     ( SRY-SRYCD(IDX2)       =  "099999903"
                                            OR "099999904"
                                            OR "099999905"
                                            OR "099999906"
                                            OR "099999907"  )
             OR     ((ACCT-SRYKBN = "40" OR "50" OR "54" OR
                                    "60" OR "64" OR "70" OR
                                    "80"                   ) AND
                     (ACCT-ZAITEN       NOT =  ZERO        ) AND
                     (ACCT-ZAIKAISU     NOT =  ZERO        ) AND
