              IF   RECE02-KOHNUM(KH-IDX - 1)  =  "051"
              AND  FLG-KOH                NOT =   2
                    MOVE     1    TO    FLG-KOH
              END-IF
              IF   RECE02-KOHNUM(KH-IDX - 1)  =  "010"
