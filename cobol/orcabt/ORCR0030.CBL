                    (SYU-JYO-COMPFTN  NOT =   ZERO         ) AND
                    (RECE02-SRYYM        >=  "201010"      ))
                OR ((SYS-1001-PREFNUM     =   04           ) AND
                    (SYU-KOH1HKNNUM       =  "463" OR "563"
                                          OR "263" OR "363") AND
                    (SYU-JYO-COMPFTN  NOT =   ZERO         ) AND
                    (RECE02-SRYYM        >=  "201201"      ))
                OR ((SYS-1001-PREFNUM     =   10           ) AND
                    (SYU-KOH1HKNNUM       =  "145" OR "245"
                                          OR "146" OR "246"
                                          OR "143" OR "243") AND
                    (SYU-JYO-COMPFTN  NOT =   ZERO         ) AND
                    (RECE02-SRYYM        >=  "201104"      ))
                OR ((SYS-1001-PREFNUM     =   12           ) AND
                    (SYU-KOH1HKNNUM       =  "263" OR "363"
                                          OR "463"         ) AND
                    (SYU-JYO-COMPFTN  NOT =   ZERO         ) AND
                    (RECE02-SRYYM        >=  "201203"      ))
