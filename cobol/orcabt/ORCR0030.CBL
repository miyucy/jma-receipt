                                     "025" OR "038"             )
              OR    ((RECE02-KOHNUM(KH-IDX - 1) =  "054"       ) AND
                     (RECE02-SRYYM             >=  "201501"    ))
                EVALUATE   RECE02-KOHNUM(KH-IDX - 1)
                  WHEN       "010"
                    COMPUTE  WRK-TAIHI-GENKIN2 = SYU-TOTAL-HKNTEN
