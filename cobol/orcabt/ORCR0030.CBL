             MOVE    "tbl_ptkohinf"    TO  MCP-TABLE
             MOVE    "key4"            TO  MCP-PATHNAME
             PERFORM  900-PTKOHINF-READ-SEC
             PERFORM  UNTIL   FLG-PTKOHINF    =   1
                IF  (PTKOH-TEKSTYMD  <=   WRK-GEN70-YMD )
