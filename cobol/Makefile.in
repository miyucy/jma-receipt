	orcabt

COPYDIRS = copy

all:   
	@set -e; for i in $(COPYDIRS) $(SUBDIRS); do $(MAKE) -C $$i all; done
filecheck:   
	@set -e; for i in $(COPYDIRS) $(SUBDIRS); do $(MAKE) -C $$i filecheck; done
clean: 
	@set -e; for i in $(COPYDIRS) $(SUBDIRS); do $(MAKE) -C $$i clean; done
distclean: 
	@set -e; for i in $(COPYDIRS) $(SUBDIRS); do $(MAKE) -C $$i distclean; done
	-rm -f Makefile Makefile.global

install installdirs:
	@set -e; for i in $(COPYDIRS) $(SUBDIRS); do $(MAKE) -C $$i $@ ; done

install-copys install-copydirs: 
	@set -e; for i in $(COPYDIRS); do $(MAKE) -C $$i $@ ; done
