                                           TO  WRK-Z9
               MOVE    WRK-Z9              TO  S03-TSKYMONY(IDX0)
               MOVE    SPA-GMN-TNYUKIN  (IDX0)
                                           TO  WRK-Z9
               MOVE    WRK-Z9              TO  S03-TNYUKINMONEY(IDX0)
               MOVE    SPA-GMN-TSYORIYMD (IDX0)
                                           TO  S03-TSYORIYMD   (IDX0)
               IF    ( SPA-NAI-TMEIJOUTAIKBN  (IDX0)   =   "1" )
                   STRING  SPA-GMN-TJOUTAI  (IDX0)
                                           DELIMITED   BY  SPACE
                           "*"             DELIMITED   BY  SIZE
                   INTO    S03-TJTI        (IDX0)
                   END-STRING
               ELSE
                   MOVE    SPA-GMN-TJOUTAI  (IDX0)
                                           TO  S03-TJTI        (IDX0)
               END-IF
               MOVE    SPA-GMN-TNYKN-HOHO(IDX0)
                                           TO  S03-TNYKN-NAME  (IDX0)
               IF    ( SPA-GMN-SMEILIST-SEL    =   IDX0 )
                   MOVE    "T"             TO  S03-SELECT (IDX0)
               ELSE
                   MOVE    "F"             TO  S03-SELECT (IDX0)
               END-IF
      *
           END-PERFORM
      *
      *    自費名称
           PERFORM     VARYING IDX0    FROM    1   BY  1      
                       UNTIL ( IDX0    >       CONST-JIHI-MAX )
               MOVE    SPA-GMN-JIHIMSG(IDX0)
                                       TO  S03-JIHIMSG (IDX0)
           END-PERFORM
      *
      *    労災保険適用分
           MOVE    SPA-GMN-RSISHOSHIN  TO  S03-RSISHOSHIN
           MOVE    SPA-GMN-RSISAISHIN  TO  S03-RSISAISHIN
           MOVE    SPA-GMN-RSISDO      TO  S03-RSISDO
           MOVE    SPA-GMN-RSIETC      TO  S03-RSIETC
      *
           MOVE    WIDGET-NORMAL       TO  S03-CHOSEIIN-STATE
                                           S03-NYUKINGK2-STATE
                                           S03-NYUKINYMD-STATE
                                           S03-NYUKINHOUHOU-STATE
                                           S03-NYUKINHOUHOULST-STATE
                                           S03-SKYPRT-STATE
                                           S03-SKYPRTLST-STATE
                                           S03-SRYPRT-STATE
                                           S03-SRYPRTLST-STATE
                                           S03-B12-STATE
                                           S03-B12S-STATE
                                           S03-NAIYO-STATE
                                           S03-NAIYOLST-STATE
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
               IF    ( SPA-NAI-MINUS  =   "1" )
                   MOVE    WIDGET-INSENSITIVE
                                           TO  S03-NYUKINHOUHOU-STATE
                                               S03-NYUKINHOUHOULST-STATE
               END-IF
           END-IF
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
            AND  ( SPA-NAI-PROC-FLG    =   3  OR 4)
               MOVE    WIDGET-INSENSITIVE
                                       TO  S03-CHOSEIIN-STATE
           END-IF
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-SYOKAI )
               MOVE    WIDGET-INSENSITIVE  TO  S03-CHOSEIIN-STATE
                                               S03-NYUKINGK2-STATE
                                               S03-NYUKINYMD-STATE
                                               S03-NYUKINHOUHOU-STATE
                                               S03-NYUKINHOUHOULST-STATE
                                               S03-SKYPRT-STATE
                                               S03-SKYPRTLST-STATE
                                               S03-SRYPRT-STATE
                                               S03-SRYPRTLST-STATE
                                               S03-B12-STATE
           END-IF
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-NYUKIN )
               MOVE    WIDGET-INSENSITIVE
                                       TO  S03-CHOSEIIN-STATE
           END-IF
      *
           IF    ( SPA-NAI-HEAD-DENPJTIKBN =   "3" )
               MOVE    WIDGET-INSENSITIVE  TO  S03-CHOSEIIN-STATE
                                               S03-NYUKINGK2-STATE
                                               S03-NYUKINYMD-STATE
                                               S03-NYUKINHOUHOU-STATE
                                               S03-NYUKINHOUHOULST-STATE
                                               S03-SKYPRT-STATE
                                               S03-SKYPRTLST-STATE
                                               S03-SRYPRT-STATE
                                               S03-SRYPRTLST-STATE
                                               S03-B12-STATE
                                               S03-NAIYO-STATE
                                               S03-NAIYOLST-STATE
      *
               MOVE    SPACE               TO  S03-B03-LABEL
      *
           ELSE
      *
               EVALUATE SPA-S01-SYORINO
      *
               WHEN    CONST-PROC-NYUKIN
                   MOVE    "履歴修正"      TO  S03-B03-LABEL
      *
               WHEN    CONST-PROC-RRKSYU
                   MOVE    "照会"          TO  S03-B03-LABEL
      *
               WHEN    CONST-PROC-SYOKAI
                   MOVE    "入金"          TO  S03-B03-LABEL
      *
               WHEN    OTHER
                   MOVE    "照会"          TO  S03-B03-LABEL
               END-EVALUATE
           END-IF
      *
           MOVE    "black"         TO  S03-LBLGENMEN-STYLE
           IF    ( SPA-NAI-DISCOUNT-KBN  NOT =   SPACE )
               MOVE    "blue"  TO  S03-LBLGENMEN-STYLE
               EVALUATE    SPA-NAI-DISCOUNT-TEIRITU
               WHEN    "1"
                   MOVE    SPA-NAI-DISCOUNT-RATE
                                   TO  WRK-ZZ9
                   STRING  "減免有（"  DELIMITED   BY  SIZE
                           WRK-ZZ9     DELIMITED   BY  SIZE
                           "％）"      DELIMITED   BY  SIZE
                   INTO    S03-LBLGENMEN
                   END-STRING
               WHEN    "2"
                   MOVE    SPA-NAI-DISCOUNT-RATE
                                   TO  WRK-Z99
                   STRING  "減免有（"  DELIMITED   BY  SIZE
                           WRK-Z99     DELIMITED   BY  SIZE
                           "円）"      DELIMITED   BY  SIZE
                   INTO    S03-LBLGENMEN
                   END-STRING
               END-EVALUATE
           END-IF
      *
           EVALUATE    FLG-SMEILIST1-ROW
           WHEN    1
               MOVE    ZERO               TO  S03-CLIST1-ROW
               MOVE    ZERO               TO  S03-CLIST1-ROWATTR
           WHEN    2
               MOVE    S03-CLIST1-COUNT   TO  S03-CLIST1-ROW
               MOVE    1                  TO  S03-CLIST1-ROWATTR
           WHEN    OTHER
               MOVE    WRK-SMEILIST1-ROW  TO  S03-CLIST1-ROW
               MOVE    ZERO               TO  S03-CLIST1-ROWATTR
           END-EVALUATE
      *
           PERFORM 5001-MAPCUR-SEC
      *
           .
       500-GMNHEN-EXT.
           EXIT.
      *
      *************************************************************
      *    画面カーソルセット処理
      *************************************************************
       5001-MAPCUR-SEC                 SECTION.
      *
           IF    ( SPA-ERRCD           =   SPACE )
             AND ( SPA-GMN-CUR         =   ZERO  )
               PERFORM 5011-INPUT-CUR-SEC
           END-IF
      *
           PERFORM 50012-CUR-SKIP-SEC
      *
           EVALUATE   SPA-GMN-CUR
           WHEN   01
               MOVE  "RIREKIIN"        TO  MCP-WIDGET
           WHEN   02
               MOVE  "NYUKINGK2"       TO  MCP-WIDGET
           WHEN   03
               MOVE  "NYUKINYMD"       TO  MCP-WIDGET
           WHEN   04
               MOVE  "NYUKINHOUHOU"    TO  MCP-WIDGET
           WHEN   05
               MOVE  "SKYPRT"          TO  MCP-WIDGET
           WHEN   07
               MOVE  "SRYPRT"          TO  MCP-WIDGET
           WHEN   06
               MOVE  "NAIYO"           TO  MCP-WIDGET
           WHEN   08
               MOVE  "CHOSEIIN"        TO  MCP-WIDGET
           WHEN   112
               MOVE  "B12"             TO  MCP-WIDGET
           END-EVALUATE
      *
           .
       5001-MAPCUR-EXT.
           EXIT.
      *
      *****************************************************************
      *    入力個所のセット処理
      *****************************************************************
       5011-INPUT-CUR-SEC              SECTION.
      *
           EVALUATE    WRK-MCP-WIDGET
           WHEN    "CHOSEIIN"
               MOVE    02              TO  SPA-GMN-CUR
           WHEN    "RIREKIIN"
               MOVE    02              TO  SPA-GMN-CUR
           WHEN    "NYUKINGK2"
               MOVE    03              TO  SPA-GMN-CUR
           WHEN    "NYUKINYMD"
               MOVE    04              TO  SPA-GMN-CUR
           WHEN    "NYUKINHOUHOU"
               MOVE    05              TO  SPA-GMN-CUR
           WHEN    "SKYPRT"
               MOVE    07              TO  SPA-GMN-CUR
           WHEN    "SRYPRT"
               MOVE    112             TO  SPA-GMN-CUR
           END-EVALUATE
      *
           .
      *
       5011-INPUT-CUR-EXT.
           EXIT.
      *****************************************************************
      *    カーソルスキップ処理
      *****************************************************************
       50012-CUR-SKIP-SEC              SECTION.
      *
           IF    ( SPA-GMN-CUR             =   8 )
               IF   ( S03-CHOSEIIN-STATE   =   WIDGET-INSENSITIVE )
                   MOVE    2           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   6 )
               IF   ( S03-NAIYO-STATE  =   WIDGET-INSENSITIVE )
                   MOVE    2           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   2 )
               IF   ( S03-NYUKINGK2-STATE  =   WIDGET-INSENSITIVE )
                   MOVE    3           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   3 )
               IF   ( S03-NYUKINYMD-STATE  =   WIDGET-INSENSITIVE )
                   MOVE    4           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   4 )
               IF   ( S03-NYUKINHOUHOU-STATE
                                           =   WIDGET-INSENSITIVE )
                   MOVE    5           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   5 )
               IF   ( S03-SKYPRT-STATE
                                           =   WIDGET-INSENSITIVE )
                   MOVE    7           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   7 )
               IF   ( S03-SRYPRT-STATE
                                           =   WIDGET-INSENSITIVE )
                   MOVE    112         TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           IF    ( SPA-GMN-CUR             =   112 )
               IF   ( S03-B12-STATE        =   WIDGET-INSENSITIVE )
                   MOVE    1           TO  SPA-GMN-CUR
               END-IF
           END-IF
      *
           .
      *
       50012-CUR-SKIP-EXT.
           EXIT.
      *************************************************************
      *    画面遷移処理
      *************************************************************
       200-GMNSENI                     SECTION.
      *
           MOVE    MCP-WIDGET          TO  WRK-MCP-WIDGET
      *
           EVALUATE    MCP-EVENT       ALSO    MCP-WIDGET
      *    戻る
           WHEN    "CLICKED"           ALSO    "B01"
               PERFORM 210-BACK
      *    処理区分
           WHEN    "CLICKED"           ALSO    "B03"
               PERFORM 430-SYORIKBN-SEC
      *    予約登録
           WHEN    "CLICKED"           ALSO    "B10"
               PERFORM 480-YOYAKU-SEC
      *    受付一覧
           WHEN    "CLICKED"           ALSO    "B11"
               PERFORM 490-UKEICHI-SEC
      *    明細書印刷
           WHEN    "CLICKED"           ALSO    "B11S"
               PERFORM 411-PRINT-SEC
      *    請求書印刷
           WHEN    "CLICKED"           ALSO    "B12S"
               PERFORM 410-PRINT-SEC
      *    登録
           WHEN    "CLICKED"           ALSO    "B12"
               PERFORM 410-TOROKU-SEC
           END-EVALUATE
           .
       200-GMNSENI-EXT.
           EXIT.
      *************************************************************
      *    会話　処理
      *************************************************************
       200-ENTRY                       SECTION.
      *
           MOVE    MCP-WIDGET      TO  WRK-MCP-WIDGET
      *
           EVALUATE    MCP-EVENT   ALSO    MCP-WIDGET
      *
           WHEN    ANY             ALSO    "clist1"
               PERFORM 4100-SMEILIST-CHK-SEC
      *
           WHEN    "ACTIVATE"      ALSO    "NAIYO"
               PERFORM 4100-SYORIKBN-CHK-SEC
      *
           WHEN    "ACTIVATE"      ALSO    "RIREKIIN"
               PERFORM 4100-SELNUM-CHK-SEC
      *
           WHEN    OTHER
      *
      *        入力個所のセット
               PERFORM 400-GMN-INPUT-SEC
      *        入力チェック処理
               PERFORM 410-INPUT-CHK-SEC
           END-EVALUATE
      *
           .
       200-ENTRY-EXT.
           EXIT.
      *
      *****************************************************************
      *    入力個所のセット処理
      *****************************************************************
       400-GMN-INPUT-SEC               SECTION.
      *
           MOVE    ZERO            TO  SPA-GMN-CUR
      *
           .
      *
       400-GMN-INPUT-EXT.
           EXIT.
      *
      *****************************************************************
      *    入力チェック処理
      *****************************************************************
       410-INPUT-CHK-SEC          SECTION.
      *
      *    画面をＳＰＡセット
           PERFORM 4101-GMN-SPA-SET-SEC
      *
      *    基本チェック処理
           PERFORM 4102-KIHON-CHK-SEC
      *
           .
      *
       410-INPUT-CHK-EXT.
           EXIT.
      *
      *****************************************************************
      *    画面ＳＰＡ編集処理
      *****************************************************************
       4101-GMN-SPA-SET-SEC            SECTION.
      *
           MOVE    S03-CHOSEIIN        TO  SPA-GMN-CHOSEIIN
           MOVE    S03-NYUKINGK2       TO  SPA-GMN-NYUKIN
           MOVE    S03-NYUKINYMD       TO  SPA-GMN-NYUKINYMD
           MOVE    S03-NYUKINHOUHOU    TO  SPA-GMN-NYKN-HOHO-G
           MOVE    S03-SKYPRT          TO  SPA-GMN-SKYPRT-G
           MOVE    S03-SRYPRT          TO  SPA-GMN-SRYPRT-G
      *
           .
      *
       4101-GMN-SPA-SET-EXT.
           EXIT.
      *
      *****************************************************************
      *    基本チェック処理
      *****************************************************************
       4102-KIHON-CHK-SEC              SECTION.
      *
           EVALUATE    SPA-S01-SYORINO
      *
           WHEN    CONST-PROC-NYUKIN
               PERFORM 4102-NYUKIN-KIHON-CHK-SEC
      *
           WHEN    CONST-PROC-RRKSYU
               PERFORM 4102-RRKSYU-KIHON-CHK-SEC
      *
           WHEN    CONST-PROC-SYOKAI
               PERFORM 4102-SYOKAI-KIHON-CHK-SEC
           END-EVALUATE
      *
           .
      *
       4102-KIHON-CHK-EXT.
           EXIT.
      *****************************************************************
      *    入金処理時基本チェック処理
      *****************************************************************
       4102-NYUKIN-KIHON-CHK-SEC       SECTION.
      *
           PERFORM 4101-NYUKIN-CHK-SEC
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-NYUKINYMD-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-NYKN-HOHO-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-SKYPRT-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-SRYPRT-CHK-SEC
           END-IF
      *
           .
       4102-NYUKIN-KIHON-CHK-EXT.
           EXIT.
      *****************************************************************
      *    履歴修正処理時基本チェック処理
      *****************************************************************
       4102-RRKSYU-KIHON-CHK-SEC       SECTION.
      *
           PERFORM 4101-SELNUM-CHK-SEC
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-CHOSEIIN-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               IF    ( SPA-NAI-MINUS  =   "1" )
                   PERFORM 4102-NYUKIN-CHK-SEC
               ELSE
                   PERFORM 4101-NYUKIN-CHK-SEC
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-NYUKINYMD-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-NYKN-HOHO-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-SKYPRT-CHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               PERFORM 4101-SRYPRT-CHK-SEC
           END-IF
      *
           .
       4102-RRKSYU-KIHON-CHK-EXT.
           EXIT.
      *****************************************************************
      *    照会処理時基本チェック処理
      *****************************************************************
       4102-SYOKAI-KIHON-CHK-SEC       SECTION.
      *
           PERFORM 4101-SKYPRT-CHK-SEC
      *
           PERFORM 4101-SRYPRT-CHK-SEC
      *
           .
       4102-SYOKAI-KIHON-CHK-EXT.
           EXIT.
      *****************************************************************
      *    収納明細リストチェック
      *****************************************************************
       4100-SMEILIST-CHK-SEC           SECTION.
      *
           INITIALIZE                  SPA-GMN-SMEILIST-SEL
                                       SPA-GMN-SELNUM
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
               INITIALIZE              SPA-GMN-INPUT-AREA
                                       SPA-NAI-INPUT-AREA
           END-IF
      *
           PERFORM VARYING IDX2    FROM    1   BY  1
                   UNTIL ( IDX2    >   SPA-GMN-SMEILIST-MAX )
      *
               IF    ( S03-SELECT (IDX2)   =   "T" )
                   MOVE    IDX2    TO  SPA-GMN-SMEILIST-SEL
                                       SPA-GMN-SELNUM
                   IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
                       PERFORM 41001-INPUT-AREA-HEN-SEC
                   END-IF
      *
                   MOVE    2       TO  SPA-GMN-CUR
      *
                   MOVE    SPA-GMN-SMEILIST-MAX
                                   TO  IDX2
               END-IF
      *
           END-PERFORM
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
               IF    ( SPA-NAI-DENPJTIKBN
                               =   " " OR "1" OR "2" OR "3"
                                OR "4" OR "6" OR "7" OR "8" OR "9" )
                   CONTINUE
               ELSE
      *
                   MOVE    "0009"  TO  SPA-ERRCD
                   MOVE    1       TO  SPA-GMN-CUR
      *
                   INITIALIZE          SPA-GMN-INPUT-AREA
                                       SPA-NAI-INPUT-AREA
      *
                   MOVE    SPA-SYSYMD  TO  WRK-SYMD
                   PERFORM 800-SEIWA-HEN-SEC
                   MOVE    WRK-HENYMDG TO  SPA-GMN-NYUKINYMD
                   MOVE    WRK-SYMD    TO  SPA-NAI-NYUKINYMD
      *
               END-IF
           END-IF
      *
           .
       4100-SMEILIST-CHK-EXT.
           EXIT.
      *****************************************************************
      *    処理区分チェック処理
      *****************************************************************
       4100-SYORIKBN-CHK-SEC           SECTION.
      *
           MOVE    S03-NAIYO       TO  SPA-GMN-SYORIKBN-G
      *
           EVALUATE SPA-GMN-SYORIKBN
           WHEN    "1"
               MOVE    CONST-PROC-NYUKIN   TO  SPA-S01-SYORINO
           WHEN    "2"
               MOVE    CONST-PROC-RRKSYU   TO  SPA-S01-SYORINO
           WHEN    OTHER
               MOVE    CONST-PROC-SYOKAI   TO  SPA-S01-SYORINO
           END-EVALUATE
      *
      *
           PERFORM 4200-SPA-SET-SEC
      *
           MOVE    1               TO  FLG-SMEILIST1-ROW
      *
           .
       4100-SYORIKBN-CHK-EXT.
           EXIT.
      *****************************************************************
      *    選択番号チェック処理
      *****************************************************************
       4100-SELNUM-CHK-SEC             SECTION.
      *
           INITIALIZE                  SPA-GMN-SMEILIST-SEL
      *
           IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
               INITIALIZE              SPA-GMN-INPUT-AREA
                                       SPA-NAI-INPUT-AREA
           END-IF
      *
           MOVE    SPA-SYSYMD      TO  WRK-SYMD
           PERFORM 800-SEIWA-HEN-SEC
           MOVE    WRK-HENYMDG     TO  SPA-GMN-NYUKINYMD
           MOVE    WRK-SYMD        TO  SPA-NAI-NYUKINYMD
      *
           MOVE    S03-RIREKIIN    TO  SPA-GMN-SELNUM
      *
           IF    ( SPA-GMN-SELNUM  NOT =   ZERO )
               PERFORM 41001-SELNUM-CHK-SEC
      *
               IF    ( SPA-ERRCD       =   SPACE )
                   IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
                       PERFORM 41001-INPUT-AREA-HEN-SEC
                   END-IF
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               MOVE    2           TO  SPA-GMN-CUR
           END-IF
      *
           .
       4100-SELNUM-CHK-EXT.
           EXIT.
      *****************************************************************
      *    選択番号チェック処理
      *****************************************************************
       41001-SELNUM-CHK-SEC            SECTION.
      *
           IF    ( SPA-GMN-SELNUM  >=  1 )
            AND  ( SPA-GMN-SELNUM  <=  SPA-GMN-SMEILIST-MAX )
               MOVE   SPA-GMN-SELNUM
                                   TO  SPA-GMN-SMEILIST-SEL
           ELSE
               MOVE    1           TO  SPA-GMN-CUR
               MOVE    "0005"      TO  SPA-ERRCD
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( SPA-S01-SYORINO     =   CONST-PROC-RRKSYU )
                   IF    ( SPA-NAI-TJOUTAIKBN (SPA-GMN-SELNUM)
                               =   " " OR "1" OR "2" OR "3"
                                OR "4" OR "6" OR "7" OR "8" OR "9" )
                       CONTINUE
                   ELSE
                       MOVE    "0009"  TO  SPA-ERRCD
                       MOVE    1       TO  SPA-GMN-CUR
                   END-IF
               END-IF
           END-IF
      *
           .
       41001-SELNUM-CHK-EXT.
           EXIT.
      *****************************************************************
      *    入力領域編集処理
      *****************************************************************
       41001-INPUT-AREA-HEN-SEC        SECTION.
      *
           MOVE    SPA-GMN-TSKYMONEY (SPA-GMN-SELNUM)
                                       TO  SPA-GMN-CHOSEIIN
                                           SPA-NAI-SKYMONEY-MOTO
      *
           MOVE    SPA-GMN-TNYUKIN   (SPA-GMN-SELNUM)
                                       TO  SPA-GMN-NYUKIN
                                           SPA-NAI-NYUKIN-MOTO
           MOVE    SPA-GMN-TSYORIYMD (SPA-GMN-SELNUM)
                                       TO  SPA-GMN-NYUKINYMD
      *
           MOVE    SPA-NAI-TNYKN-HOHO(SPA-GMN-SELNUM)
                                       TO  WRK-CMB-CD
           PERFORM 4200-NYKN-HOHOLST-CHK-SEC
           MOVE    WRK-CMB-ITM         TO  SPA-GMN-NYKN-HOHO-G
      *
           MOVE    SPA-NAI-TSYORIYMD (SPA-GMN-SELNUM)
                                       TO  SPA-NAI-NYUKINYMD
           MOVE    SPA-GMN-TDENPEDANUM(SPA-GMN-SELNUM)
                                       TO  SPA-NAI-DENPEDANUM
           MOVE    SPA-NAI-TSYURRKNUM (SPA-GMN-SELNUM)
                                       TO  SPA-NAI-SYURRKNUM
           MOVE    SPA-NAI-TSYUEDANUM (SPA-GMN-SELNUM)
                                       TO  SPA-NAI-SYUEDANUM
           MOVE    SPA-NAI-TSYURRKUPDKBN (SPA-GMN-SELNUM)
                                       TO  SPA-NAI-SYURRKUPDKBN
           MOVE    SPA-NAI-TJOUTAIKBN(SPA-GMN-SELNUM)
                                       TO  SPA-NAI-DENPJTIKBN
           MOVE    SPA-NAI-TMAINUS    (SPA-GMN-SELNUM)
                                       TO  SPA-NAI-MINUS
      *
           MOVE    ZERO                TO  SPA-NAI-PROC-FLG
      *
           EVALUATE    SPA-NAI-DENPJTIKBN
           WHEN    " "
           WHEN    "1"
           WHEN    "9"
               MOVE    1               TO  SPA-NAI-PROC-FLG
           WHEN    "3"
           WHEN    "7"
           WHEN    "8"
               MOVE    2               TO  SPA-NAI-PROC-FLG
           WHEN    "2"
               MOVE    3               TO  SPA-NAI-PROC-FLG
           WHEN    "4"
           WHEN    "6"
               MOVE    4               TO  SPA-NAI-PROC-FLG
           END-EVALUATE
      *
           .
       41001-INPUT-AREA-HEN-EXT.
           EXIT.
      *****************************************************************
      *    選択番号チェック処理
      *****************************************************************
       4101-SELNUM-CHK-SEC             SECTION.
      *
           PERFORM 41011-SELNUM-CHK-SEC
      *
           IF    ( SPA-ERRCD   NOT =   SPACE )
      *
               INITIALIZE              SPA-GMN-INPUT-AREA
                                       SPA-NAI-INPUT-AREA
      *
               MOVE    SPA-SYSYMD  TO  WRK-SYMD
               PERFORM 800-SEIWA-HEN-SEC
               MOVE    WRK-HENYMDG TO  SPA-GMN-NYUKINYMD
               MOVE    WRK-SYMD    TO  SPA-NAI-NYUKINYMD
           END-IF
      *
           .
       4101-SELNUM-CHK-EXT.
           EXIT.
      *****************************************************************
      *    選択番号チェック処理
      *****************************************************************
       41011-SELNUM-CHK-SEC            SECTION.
      *
           MOVE    ZERO            TO  SPA-GMN-SMEILIST-SEL
      *
           IF    ( SPA-GMN-SELNUM      =   ZERO )
               MOVE    1           TO  SPA-GMN-CUR
               MOVE    "0006"      TO  SPA-ERRCD
           ELSE
               PERFORM 41001-SELNUM-CHK-SEC
           END-IF
      *
           .
       41011-SELNUM-CHK-EXT.
           EXIT.
      *****************************************************************
      *    調整金チェック処理
      *****************************************************************
       4101-CHOSEIIN-CHK-SEC           SECTION.
      *
           COMPUTE WRK-SKYMONEY
               =   SPA-GMN-SKYMONEY
               -   SPA-NAI-SKYMONEY-MOTO
               +   SPA-GMN-CHOSEIIN
      *
           IF    ( WRK-SKYMONEY    <   ZERO )
               MOVE    "0016"      TO  SPA-ERRCD
               MOVE    8           TO  SPA-GMN-CUR
           END-IF
      *
           .
       4101-CHOSEIIN-CHK-EXT.
           EXIT.
      *****************************************************************
      *    入金額チェック処理
      *****************************************************************
       4101-NYUKIN-CHK-SEC             SECTION.
      *
           MOVE    ZERO            TO  WRK-NYUKIN
      *
           EVALUATE    SPA-S01-SYORINO
           WHEN    CONST-PROC-NYUKIN
               COMPUTE WRK-NYUKIN
                   =   SPA-GMN-NYUKIN-TTL
                   +   SPA-GMN-NYUKIN
           WHEN    CONST-PROC-RRKSYU
      *
               COMPUTE WRK-NYUKIN
                   =   SPA-GMN-NYUKIN-TTL
                   -   SPA-NAI-NYUKIN-MOTO
                   +   SPA-GMN-NYUKIN
           END-EVALUATE
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( SPA-GMN-NYUKIN  <  ZERO )
                   MOVE    2       TO  SPA-GMN-CUR
                   MOVE    "0014"  TO  SPA-ERRCD
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( WRK-NYUKIN  <  ZERO )
                   MOVE    2       TO  SPA-GMN-CUR
                   MOVE    "0008"  TO  SPA-ERRCD
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( WRK-MCP-WIDGET  =   "NYUKINGK2" )
                   IF    ( SPA-S01-SYORINO     =   CONST-PROC-NYUKIN )
                    AND  ( SPA-GMN-NYUKIN      =   ZERO )
                    AND  ( SPA-GMN-MISYU-ZAN   >   ZERO )
                       MOVE    SPA-GMN-MISYU-ZAN
                                   TO  SPA-GMN-NYUKIN
                   END-IF
               END-IF
           END-IF
      *
           .
       4101-NYUKIN-CHK-EXT.
           EXIT.
      *****************************************************************
      *    入金額チェック処理（入金取消、返金の履歴修正）
      *****************************************************************
       4102-NYUKIN-CHK-SEC             SECTION.
      *
           COMPUTE WRK-NYUKIN
               =   SPA-GMN-NYUKIN-TTL
               -   SPA-NAI-NYUKIN-MOTO
               +   SPA-GMN-NYUKIN
      *
           IF    ( SPA-GMN-NYUKIN  >   ZERO )
               MOVE    2           TO  SPA-GMN-CUR
               MOVE    "0013"      TO  SPA-ERRCD
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( WRK-NYUKIN  <  ZERO )
                   MOVE    2           TO  SPA-GMN-CUR
                   MOVE    "0014"      TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       4102-NYUKIN-CHK-EXT.
           EXIT.
      *****************************************************************
      *    入金日チェック処理
      *****************************************************************
       4101-NYUKINYMD-CHK-SEC          SECTION.
      *
           MOVE    SPACE                   TO  SPA-NAI-NYUKINYMD
      *
           IF    ( SPA-GMN-NYUKINYMD   =   SPACE )
               MOVE    SPA-SYSYMD          TO  WRK-SYMD
               PERFORM 800-SEIWA-HEN-SEC
               MOVE    WRK-HENYMDG         TO  SPA-GMN-NYUKINYMD
               MOVE    WRK-SYMD            TO  SPA-NAI-NYUKINYMD
           ELSE
               MOVE    SPA-GMN-NYUKINYMD   TO  WRK-SYMD
               PERFORM 800-SEIWA-HEN-SEC
               IF    ( FLG-HIZUKE-ERR      =   ZERO )
                   MOVE    WRK-HENYMDG     TO  SPA-GMN-NYUKINYMD
                   MOVE    WRK-SYMD        TO  SPA-NAI-NYUKINYMD
               ELSE
                   MOVE    3               TO  SPA-GMN-CUR
                   MOVE    "0010"          TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       4101-NYUKINYMD-CHK-EXT.
           EXIT.
      *****************************************************************
      *    入金方法チェック処理
      *****************************************************************
       4101-NYKN-HOHO-CHK-SEC          SECTION.
      *
           IF    ( SPA-GMN-NYKN-HOHO-G     =   SPACE )
               CONTINUE
           ELSE
               MOVE    SPA-GMN-NYKN-HOHO
                                       TO  WRK-CMB-CD
               PERFORM 4200-NYKN-HOHOLST-CHK-SEC
               IF    (  WRK-CMB-ITM    =   SPACE )
                   MOVE    4           TO  SPA-GMN-CUR
                   MOVE    "0011"      TO  SPA-ERRCD
               ELSE
                   MOVE WRK-CMB-ITM    TO  SPA-GMN-NYKN-HOHO-G
               END-IF
           END-IF
      *
           .
       4101-NYKN-HOHO-CHK-EXT.
           EXIT.
      *****************************************************************
      *    請求書発行コンボチェック処理
      *****************************************************************
       4101-SKYPRT-CHK-SEC             SECTION.
      *
           MOVE    1                   TO  IDX-SKY
      *
           IF    ( SPA-GMN-SKYPRT-G    =   SPACE )
               MOVE    SPA-GMN-SKYPRTLST (IDX-SKY)
                                       TO  SPA-GMN-SKYPRT-G
           ELSE
               MOVE    SPA-GMN-SKYPRT  TO  WRK-CMB-CD
               PERFORM 4200-SKYPRTLST-CHK-SEC
               IF    ( WRK-CMB-ITM     =   SPACE )
                   MOVE    SPA-GMN-SKYPRTLST (IDX-SKY)
                                       TO  SPA-GMN-SKYPRT-G
               ELSE
                   MOVE    WRK-CMB-ITM TO  SPA-GMN-SKYPRT-G
               END-IF
           END-IF
      *
           IF    ( SPA-NAI-HEAD-DENPJTIKBN =   "3" )
               MOVE    SPACE           TO  SPA-GMN-SKYPRT-G
           END-IF
      *
           .
       4101-SKYPRT-CHK-EXT.
           EXIT.
      *****************************************************************
      *    明細書発行コンボチェック処理
      *****************************************************************
       4101-SRYPRT-CHK-SEC             SECTION.
      *
           MOVE    1                   TO  IDX-SRY
      *
           IF    ( SPA-GMN-SRYPRT-G    =   SPACE )
               MOVE    SPA-GMN-SRYPRTLST (IDX-SRY)
                                       TO  SPA-GMN-SRYPRT-G
           ELSE
               MOVE    SPA-GMN-SRYPRT  TO  WRK-CMB-CD
               PERFORM 4200-SRYPRTLST-CHK-SEC
               IF    ( WRK-CMB-ITM     =   SPACE )
                   MOVE    SPA-GMN-SRYPRTLST (IDX-SRY)
                                       TO  SPA-GMN-SRYPRT-G
               ELSE
                   MOVE    WRK-CMB-ITM TO  SPA-GMN-SRYPRT-G
               END-IF
           END-IF
      *
           IF    ( SPA-NAI-HEAD-DENPJTIKBN =   "3" )
               MOVE    SPACE           TO  SPA-GMN-SRYPRT-G
           END-IF
      *
           .
       4101-SRYPRT-CHK-EXT.
           EXIT.
      *************************************************************
      *    請求書印刷処理
      *************************************************************
       410-PRINT-SEC                   SECTION.
      *
           PERFORM 4101-GMN-SPA-SET-SEC
      *
           IF    ( SPA-GMN-SELNUM  =   ZERO )
               MOVE    "0006"      TO  SPA-ERRCD
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( SPA-GMN-SELNUM  >=  1 )
                AND  ( SPA-GMN-SELNUM  <=  SPA-GMN-SMEILIST-MAX )
                   MOVE    SPA-GMN-TDENPEDANUM(SPA-GMN-SELNUM)
                                   TO  WRK-SKYPRT-DENPEDANUM
      *            印刷制御情報取得サブ
                   CALL    "ORCSONPRTSET"  USING   SPA-AREA
                   PERFORM 4101-SKYPRT-PRINT-SEC
               ELSE
                   MOVE    1       TO  SPA-GMN-CUR
                   MOVE    "0005"  TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       410-PRINT-EXT.
           EXIT.
      *************************************************************
      *    請求書兼領収書印刷処理
      *************************************************************
       4101-SKYPRT-PRINT-SEC           SECTION.
      *
           INITIALIZE                  WRK-SKYPRT-SKYMONEY
                                       WRK-SKYPRT-NYUKIN
      *
           MOVE    ZERO                TO  FLG-SYUMEI
           INITIALIZE                      SYUMEI-REC
           MOVE    SPA-HOSPNUM          TO  SMEI-HOSPNUM
           MOVE    SPA-S01-NYUGAIKBN   TO  SMEI-NYUGAIKBN
           MOVE    SPA-S01-PTID        TO  SMEI-PTID
           MOVE    SPA-S01-DENPNUM     TO  SMEI-DENPNUM
           MOVE    SYUMEI-REC          TO  MCPDATA-REC
           MOVE    "tbl_syumei"        TO  MCP-TABLE
           MOVE    "key6"              TO  MCP-PATHNAME
           PERFORM 910-DBSELECT-SEC
           IF     ( MCP-RC             =   ZERO )
               MOVE    MCPDATA-REC     TO  SYUMEI-REC
           ELSE
               INITIALIZE                  SYUMEI-REC
               MOVE    1               TO  FLG-SYUMEI
           END-IF
      *
      *
           PERFORM UNTIL ( FLG-SYUMEI  NOT =   ZERO )
                    OR   ( SMEI-DENPEDANUM >   WRK-SKYPRT-DENPEDANUM )
      *
               COMPUTE WRK-SKYPRT-SKYMONEY
                   =   WRK-SKYPRT-SKYMONEY
                   +   SMEI-SKYMONEY
      *
               IF    ( SMEI-DENPEDANUM     =   WRK-SKYPRT-DENPEDANUM )
                   MOVE    SMEI-NYUHEN-MONEY
                                           TO  WRK-SKYPRT-NYUKIN
               END-IF
      *
               IF    ( SMEI-DENPEDANUM     <   WRK-SKYPRT-DENPEDANUM )
                   COMPUTE WRK-SKYPRT-SKYMONEY
                       =   WRK-SKYPRT-SKYMONEY
                       -   SMEI-NYUHEN-MONEY
               END-IF
      *
               MOVE    "tbl_syumei"    TO  MCP-TABLE
               MOVE    "key6"          TO  MCP-PATHNAME
               PERFORM 910-DBFETCH-SEC
               IF     ( MCP-RC         =   ZERO )
                   MOVE    MCPDATA-REC TO  SYUMEI-REC
               ELSE
                   INITIALIZE              SYUMEI-REC
                   MOVE    1           TO  FLG-SYUMEI
               END-IF
      *
           END-PERFORM
      *
           MOVE    "tbl_syumei"        TO  MCP-TABLE
           MOVE    "key6"              TO  MCP-PATHNAME
           PERFORM 910-DBCLOSECURSOR-SEC
      *
           MOVE    SPA-AREA            TO  WRK-SPA-AREA
           MOVE    SPA-S01-SRYYMD      TO  WRK-SPA-SRYYMD
           MOVE    SPA-S01-NYUGAIKBN   TO  WRK-SPA-NYUGAIKBN
           MOVE    SPA-S01-PTID        TO  WRK-SPA-PTID
           MOVE    SPA-S01-PTNUM       TO  WRK-SPA-PTNUM
           MOVE    SPA-S01-NAME        TO  WRK-SPA-NAME
      *
      *    労災判定
           IF   (  SPA-S01-HKNNUM      =   "971" OR "973" )
               MOVE    1               TO  WRK-SPA-HKNKBN
           ELSE
               MOVE    ZERO            TO  WRK-SPA-HKNKBN
           END-IF
      *
           MOVE    SPA-S01-RSI-HKNKBN  TO  WRK-SPA-RSI-HKNKBN
      *
           PERFORM 41011-PRINT-PG-SEC
      *
           INITIALIZE      ORCHC03AREA
           MOVE   SPA-S01-DENPNUM      TO  ORCHC03-DENPNUM
           MOVE   WRK-SKYPRT-SKYMONEY  TO  ORCHC03-SEIKYU
      *    MOVE   ????                 TO  ORCHC03-SEIKYU-TAX-SAI
           MOVE   WRK-SKYPRT-NYUKIN    TO  ORCHC03-NYUKIN
      *
           MOVE    "2"                 TO  WRK-NYUGAIKBN
           PERFORM 3101-MISYU-SUM-SEC
      *
           PERFORM 900-SYUNOU-KEY-SEL-SEC
      *
           IF    ( SYS-1039-S03SKYMISYUPRTKBN  =   "0" )
               CONTINUE
           ELSE
               COMPUTE ORCHC03-ZENMISYU
                   =   WRK-MISYU
                   - ( SYU-SKYMONEY        -   SYU-NYUKIN-TOTAL )
      *
               IF    ( ORCHC03-ZENMISYU    <   ZERO )
                   MOVE    ORCHC03-ZENMISYU
                                       TO  ORCHC03-HENKIN
                   MOVE    ZERO        TO  ORCHC03-ZENMISYU
               END-IF
      *
               MOVE    WRK-MISYU-P     TO  ORCHC03-ZENMISYU-2
      *
               COMPUTE ORCHC03-KANYUKIN
                   =   WRK-MISYU-M         *   -1
      *
               IF    ( SYU-SKYMONEY        <   SYU-NYUKIN-TOTAL )
                   COMPUTE ORCHC03-KANYUKIN
                       =   ORCHC03-KANYUKIN
                       - ( SYU-NYUKIN-TOTAL    -   SYU-SKYMONEY )
               ELSE
                   COMPUTE ORCHC03-ZENMISYU-2
                       =   ORCHC03-ZENMISYU-2
                       - ( SYU-SKYMONEY        -   SYU-NYUKIN-TOTAL )
               END-IF
           END-IF
      *
           MOVE   3                    TO  ORCHC03-HAKKOFLG
           MOVE   WRK-SKYPRT-DENPEDANUM
                                       TO  ORCHC03-DENPEDANUM
           MOVE  "01"                  TO  WRK-SPA-PRT-FLG
           CALL  WRK-SEIKYU-PG     USING   WRK-SPA-AREA
                                           ORCHC03AREA
           MOVE  SPACE                 TO  WRK-SPA-PRT-FLG
      *
           .
       4101-SKYPRT-PRINT-EXT.
           EXIT.
      *****************************************************************
      *    帳票プログラム取得処理
      *****************************************************************
       41011-PRINT-PG-SEC              SECTION.
      *
      *    請求書
           MOVE    SPACE               TO  WRK-SEIKYU-PG
      *
           INITIALIZE                      ORCSPRTNMAREA
           MOVE    "1"                 TO  ORCSPRTNM-KBN
           MOVE    "00000003"          TO  ORCSPRTNM-KBNCD
           MOVE    SPA-SYSYMD          TO  ORCSPRTNM-SRYYMD
           MOVE    "2"                 TO  ORCSPRTNM-NYUGAIKBN
           CALL    "ORCSPRTNM"         USING
                                           ORCSPRTNMAREA
                                           WRK-SPA-AREA
                                           SYS-1032-REC
           IF    ( ORCSPRTNM-RC        =   ZERO )
              MOVE    ORCSPRTNM-PRTPG  TO  WRK-SEIKYU-PG
           ELSE
              MOVE    "ORCHC03"        TO  WRK-SEIKYU-PG
           END-IF
      *
           .
      *
       41011-PRINT-PG-EXT.
           EXIT.
      *************************************************************
      *    明細書印刷処理
      *************************************************************
       411-PRINT-SEC                   SECTION.
      *
           PERFORM 4101-GMN-SPA-SET-SEC
      *
           MOVE    ZERO            TO  WRK-SRYPRT-DENPEDANUM
      *
           IF    ( SPA-GMN-SELNUM  >=  1 )
            AND  ( SPA-GMN-SELNUM  <=  SPA-GMN-SMEILIST-MAX )
               MOVE    SPA-GMN-TDENPEDANUM(SPA-GMN-SELNUM)
                                   TO  WRK-SRYPRT-DENPEDANUM
           END-IF
      *
      *    印刷制御情報取得サブ
           CALL    "ORCSONPRTSET"  USING   SPA-AREA
      *
           PERFORM 4111-SRYPRT-PRINT-SEC
      *
           .
       411-PRINT-EXT.
           EXIT.
      *************************************************************
      *    明細書印刷処理
      *************************************************************
       4111-SRYPRT-PRINT-SEC           SECTION.
      *
           MOVE    SPA-AREA            TO  WRK-SPA-AREA
           MOVE    SPA-S01-SRYYMD      TO  WRK-SPA-SRYYMD
           MOVE    SPA-S01-NYUGAIKBN   TO  WRK-SPA-NYUGAIKBN
           MOVE    SPA-S01-PTID        TO  WRK-SPA-PTID
           MOVE    SPA-S01-PTNUM       TO  WRK-SPA-PTNUM
           MOVE    SPA-S01-NAME        TO  WRK-SPA-NAME
      *
           PERFORM 41111-PRINT-PG-SEC
      *
           INITIALIZE                      ORCHC04AREA
      *
           MOVE   SPA-S01-DENPNUM      TO  ORCHC04-DENPNUM
           MOVE   3                    TO  ORCHC04-HAKKOFLG
           MOVE    "02"                TO  WRK-SPA-PRT-FLG
           CALL    WRK-MEISAI-PG           USING
                                           WRK-SPA-AREA
                                           ORCHC04AREA
           MOVE   SPACE                TO  WRK-SPA-PRT-FLG
      *
           .
       4111-SRYPRT-PRINT-EXT.
           EXIT.
      *****************************************************************
      *    帳票プログラム取得処理
      *****************************************************************
       41111-PRINT-PG-SEC              SECTION.
      *
      *    明細
           MOVE    SPACE               TO  WRK-MEISAI-PG
      *
           INITIALIZE                      ORCSPRTNMAREA
           MOVE    "1"                 TO  ORCSPRTNM-KBN
           MOVE    "00000008"          TO  ORCSPRTNM-KBNCD
           MOVE    SPA-SYSYMD          TO  ORCSPRTNM-SRYYMD
           MOVE    "2"                 TO  ORCSPRTNM-NYUGAIKBN
           CALL    "ORCSPRTNM"         USING
                                           ORCSPRTNMAREA
                                           WRK-SPA-AREA
                                           SYS-1032-REC
           IF    ( ORCSPRTNM-RC        =   ZERO )
              MOVE    ORCSPRTNM-PRTPG  TO  WRK-MEISAI-PG
           ELSE
              MOVE    "ORCHC04"        TO  WRK-MEISAI-PG
           END-IF
      *
           .
      *
       41111-PRINT-PG-EXT.
           EXIT.
      *************************************************************
      *    登録処理
      *************************************************************
       410-TOROKU-SEC                  SECTION.
      *
           PERFORM 410-INPUT-CHK-SEC
      *
           IF    ( SPA-ERRCD       =   SPACE )
               PERFORM 4901-KANRENCHK-SEC
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               MOVE    ZERO        TO  WRK-NYUKIN
                                       WRK-SKYMONEY
               IF    ( SPA-S01-SYORINO
                                   =   CONST-PROC-RRKSYU )
                   COMPUTE WRK-SKYMONEY
                       =   SPA-GMN-SKYMONEY
                       -   SPA-NAI-SKYMONEY-MOTO
                       +   SPA-GMN-CHOSEIIN
      *
                   COMPUTE WRK-NYUKIN
                       =   SPA-GMN-NYUKIN-TTL
                       -   SPA-NAI-NYUKIN-MOTO
                       +   SPA-GMN-NYUKIN
               ELSE
                   COMPUTE WRK-SKYMONEY
                       =   SPA-GMN-SKYMONEY
                       +   SPA-GMN-CHOSEIIN
      *
                   COMPUTE WRK-NYUKIN
                       =   SPA-GMN-NYUKIN-TTL
                       +   SPA-GMN-NYUKIN
               END-IF
      *
               IF    ( SPA-NAI-PROC-FLG    =   4 )
                       PERFORM 4101-TOROKU-SEC
               ELSE
                   IF    ( WRK-SKYMONEY    <   WRK-NYUKIN )
                       MOVE    "2001"  TO  SPA-SIDCD
                   ELSE
                       PERFORM 4101-TOROKU-SEC
                   END-IF
               END-IF
      *
           END-IF
      *
           .
       410-TOROKU-EXT.
           EXIT.
      *
      *************************************************************
      *    登録処理
      *************************************************************
       4101-TOROKU-SEC                 SECTION.
      *
           IF    ( SPA-ERRCD       =   SPACE )
               EVALUATE    SPA-S01-SYORINO
               WHEN    CONST-PROC-NYUKIN
                   PERFORM   4902-NYUKIN-KOUSIN-SEC
               WHEN    CONST-PROC-RRKSYU
                   PERFORM   4902-NYUKIN-RRKSYU-SEC
               END-EVALUATE
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
      *
      *        印刷制御情報取得サブ
               CALL    "ORCSONPRTSET"  USING   SPA-AREA
      *
               IF    ( SPA-GMN-SKYPRT  =   "1" )
                   MOVE    SMEI-DENPEDANUM
                                   TO  WRK-SKYPRT-DENPEDANUM
                   PERFORM 4101-SKYPRT-PRINT-SEC
               END-IF
      *
               IF    ( SPA-GMN-SRYPRT  =   "1" )
                   MOVE    SMEI-DENPEDANUM
                                   TO  WRK-SRYPRT-DENPEDANUM
                   PERFORM 4111-SRYPRT-PRINT-SEC
               END-IF
      *
               PERFORM 4200-SPA-SET-SEC
      *
               EVALUATE    SPA-S01-SYORINO
               WHEN    CONST-PROC-NYUKIN
                   MOVE    2       TO  FLG-SMEILIST1-ROW
                   MOVE    2       TO  SPA-GMN-CUR
               WHEN    CONST-PROC-RRKSYU
                   MOVE    1       TO  SPA-GMN-CUR
               END-EVALUATE
           END-IF
      *
           .
       4101-TOROKU-EXT.
           EXIT.
      *************************************************************
      *    関連チェック
      *************************************************************
       4901-KANRENCHK-SEC              SECTION.
      *
           EVALUATE    SPA-S01-SYORINO
           WHEN    CONST-PROC-NYUKIN
               PERFORM 4901-NYUKIN-CHK-SEC
           WHEN    CONST-PROC-RRKSYU
               PERFORM 4901-RRKSYU-CHK-SEC
           END-EVALUATE
      *
           .
       4901-KANRENCHK-EXT.
           EXIT.
      *************************************************************
      *    入金処理時関連チェック処理
      *************************************************************
       4901-NYUKIN-CHK-SEC             SECTION.
      *
           IF    ( SPA-GMN-NYUKIN      =   ZERO )
               MOVE    2               TO  SPA-GMN-CUR
               MOVE    "0007"          TO  SPA-ERRCD
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               IF    ( SPA-NAI-NYUKINYMD   =   SPACE )
                   MOVE    3           TO  SPA-GMN-CUR
                   MOVE    "0012"      TO  SPA-ERRCD
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               IF    ( SPA-GMN-NYKN-HOHO-G =   SPACE )
                   MOVE    4           TO  SPA-GMN-CUR
                   MOVE    "0004"      TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       4901-NYUKIN-CHK-EXT.
           EXIT.
      *************************************************************
      *    履歴修正処理時関連チェック処理
      *************************************************************
       4901-RRKSYU-CHK-SEC             SECTION.
      *
           IF    ( SPA-GMN-SELNUM      =   ZERO )
               MOVE    1               TO  SPA-GMN-CUR
               MOVE    "0006"          TO  SPA-ERRCD
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               IF    ( SPA-NAI-NYUKINYMD   =   SPACE )
                   MOVE    3           TO  SPA-GMN-CUR
                   MOVE    "0012"      TO  SPA-ERRCD
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD           =   SPACE )
               IF    ( SPA-GMN-NYUKIN  >   ZERO )
                AND  ( SPA-GMN-NYKN-HOHO-G =   SPACE )
                       MOVE    4           TO  SPA-GMN-CUR
                       MOVE    "0004"      TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
      *
       4901-RRKSYU-CHK-EXT.
           EXIT.
      *************************************************************
      *    入金編集処理
      *************************************************************
       4902-NYUKIN-KOUSIN-SEC          SECTION.
      *
           PERFORM 49021-SYUNOU-UPD-SEC
      *
           IF    ( SPA-ERRCD       =   SPACE )
               PERFORM 49021-SYUMEI-INS-SEC
           END-IF
      *
           .
       4902-NYUKIN-KOUSIN-EXT.
           EXIT.
      *************************************************************
      *    履歴修正編集処理
      *************************************************************
       4902-NYUKIN-RRKSYU-SEC          SECTION.
      *
           PERFORM 49021-SYUNOU-UPD-SEC
      *
           IF    ( SPA-ERRCD       =   SPACE )
               IF    ( SPA-NAI-PROC-FLG    =   1  OR  2 )
                   PERFORM 49021-SYUTTL-UPD-SEC
               END-IF
           END-IF
      *
           IF    ( SPA-ERRCD       =   SPACE )
               PERFORM 49021-SYUMEI-UPD-SEC
           END-IF
      *
           .
       4902-NYUKIN-RRKSYU-EXT.
           EXIT.
      *************************************************************
      *    収納マスタ更新処理
      *************************************************************
       49021-SYUNOU-UPD-SEC            SECTION.
      *
           PERFORM 900-SYUNOU-KEY-SEL-SEC
           IF    ( FLG-SYUNOU      NOT =   ZERO )
               MOVE    "1001"          TO  SPA-ERRCD
           ELSE
      *
               EVALUATE    SPA-S01-SYORINO
               WHEN    CONST-PROC-NYUKIN
                   COMPUTE SYU-NYUKIN-TOTAL
                       =   SYU-NYUKIN-TOTAL
                       +   SPA-GMN-NYUKIN
               WHEN    CONST-PROC-RRKSYU
      *
                   IF    ( SPA-NAI-PROC-FLG    =   1  OR  2 )
                       COMPUTE SYU-SKYMONEY
                           =   SYU-SKYMONEY
                           -   SPA-NAI-SKYMONEY-MOTO
                           +   SPA-GMN-CHOSEIIN
                       COMPUTE SYU-CHOSEI
                           =   SYU-CHOSEI
                           -   SPA-NAI-SKYMONEY-MOTO
                           +   SPA-GMN-CHOSEIIN
                   END-IF
      *
                   COMPUTE SYU-NYUKIN-TOTAL
                       =   SYU-NYUKIN-TOTAL
                       -   SPA-NAI-NYUKIN-MOTO
                       +   SPA-GMN-NYUKIN
               END-EVALUATE
      *
               COMPUTE SYU-NYUKIN-KAISU    =   SYU-NYUKIN-KAISU    +   1
      *
               IF    ( SYU-SKYMONEY    =   ZERO )
                   MOVE    SPACE           TO  SYU-DENPJTIKBN
               ELSE
                   IF    ( SYU-SKYMONEY    >   SYU-NYUKIN-TOTAL )
                       MOVE    "2"         TO  SYU-DENPJTIKBN
                   ELSE
                       MOVE    "1"         TO  SYU-DENPJTIKBN
                   END-IF
               END-IF
      *
               PERFORM 800-MCNDATE-SEC
               MOVE    SMCNDATE-YMD        TO  SYU-UPYMD
               MOVE    SMCNDATE-HMS        TO  SYU-UPHMS
      *
               INITIALIZE                  SS002-AREA
               MOVE    "2"                 TO  SS002-UPDKBN
               MOVE    SPA-NAI-SYURRKNUM   TO  SS002-IN-SYURRKNUM
               MOVE    SPA-NAI-SYUEDANUM   TO  SS002-IN-SYUEDANUM
               MOVE    SPA-NAI-SYURRKUPDKBN TO SS002-IN-SYURRKUPDKBN
               CALL    "ORCSS002"  USING   SS002-AREA
                                           SYUNOU-REC
                                           SPA-AREA
               IF    ( SS002-RC    NOT =   ZERO )
                   GO  TO  49021-SYUNOU-UPD-EXT
               END-IF
      *
               MOVE    SYUNOU-REC          TO  MCPDATA-REC
               MOVE    "tbl_syunou"        TO  MCP-TABLE
               MOVE    "key"               TO  MCP-PATHNAME
               PERFORM 910-DBUPDATE-SEC
               IF    ( MCP-RC              NOT =   ZERO )
                   MOVE    "1002"          TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       49021-SYUNOU-UPD-EXT.
           EXIT.
      *************************************************************
      *    収納合計マスタ更新処理
      *************************************************************
       49021-SYUTTL-UPD-SEC            SECTION.
      *
           PERFORM 900-SYUTOTAL-KEY-SEL-SEC
           IF    ( FLG-SYUTTL      =   ZERO )
      *
               COMPUTE SYUTTL-SKYMONEY
                   =   SYUTTL-SKYMONEY
                   -   SPA-NAI-SKYMONEY-MOTO
                   +   SPA-GMN-CHOSEIIN
               COMPUTE SYUTTL-CHOSEI
                   =   SYUTTL-CHOSEI
                   -   SPA-NAI-SKYMONEY-MOTO
                   +   SPA-GMN-CHOSEIIN
      *
               MOVE    SYUTOTAL-REC        TO  MCPDATA-REC
               MOVE    "tbl_syutotal"      TO  MCP-TABLE
               MOVE    "key"               TO  MCP-PATHNAME
               PERFORM 910-DBUPDATE-SEC
               IF    ( MCP-RC              NOT =   ZERO )
                   MOVE    "1003"          TO  SPA-ERRCD
               END-IF
      *
           END-IF
      *
           .
       49021-SYUTTL-UPD-EXT.
           EXIT.
      *************************************************************
      *    収納明細作成処理
      *************************************************************
       49021-SYUMEI-INS-SEC            SECTION.
      *
           MOVE    ZERO                TO  FLG-SYUMEI
           INITIALIZE                      SYUMEI-REC
           MOVE    SPA-HOSPNUM          TO  SMEI-HOSPNUM
           MOVE    SPA-S01-NYUGAIKBN   TO  SMEI-NYUGAIKBN
           MOVE    SPA-S01-PTID        TO  SMEI-PTID
           MOVE    SPA-S01-DENPNUM     TO  SMEI-DENPNUM
           MOVE    SYUMEI-REC          TO  MCPDATA-REC
           MOVE    "tbl_syumei"        TO  MCP-TABLE
           MOVE    "key7"              TO  MCP-PATHNAME
           PERFORM 910-DBSELECT-SEC
           IF     ( MCP-RC             =   ZERO )
               MOVE    MCPDATA-REC     TO  SYUMEI-REC
           ELSE
               INITIALIZE                  SYUMEI-REC
               MOVE    1               TO  FLG-SYUMEI
           END-IF
      *
           MOVE    "tbl_syumei"        TO  MCP-TABLE
           MOVE    "key7"              TO  MCP-PATHNAME
           PERFORM 910-DBCLOSECURSOR-SEC
      *
           IF    ( FLG-SYUMEI      NOT =   ZERO )
               MOVE    "1003"          TO  SPA-ERRCD
           ELSE
      *
               COMPUTE WRK-DENPEDANUM  =   SMEI-DENPEDANUM +   1
      *
               INITIALIZE                  SYUMEI-REC
      *
               MOVE    SPA-HOSPNUM          TO  SMEI-HOSPNUM
               MOVE    SPA-S01-NYUGAIKBN   TO  SMEI-NYUGAIKBN
               MOVE    SPA-S01-PTID        TO  SMEI-PTID
      *
      *        伝票番号
               MOVE    SPA-S01-DENPNUM     TO  SMEI-DENPNUM
      *
      *        伝票番号枝番
               MOVE    WRK-DENPEDANUM      TO  SMEI-DENPEDANUM
      *
      *        入金連番
               MOVE    ZERO                TO  SMEI-NYUKINRENNUM
      *
      *        診療科
               MOVE    SPA-NAI-SRYKA       TO  SMEI-SRYKA
      *
      *        伝票状態区分
               MOVE    SPACE               TO  SMEI-MEISAIJOUTAIKBN
      *
      *        請求発行日（発行日の西暦）
               MOVE    SPACE               TO  SMEI-SKYPRINTYMD
      *
      *        請求再発行日
               MOVE    SPACE               TO  SMEI-SKYREPRINTYMD
      *
      *        領収書発行日
               MOVE    SPACE               TO  SMEI-RYOSYUPRINTYMD
      *
      *        領収書再発行日
               MOVE    SPACE               TO  SMEI-RYOSYUREPRINTYMD
      *
      *        請求金額
               MOVE    ZERO                TO  SMEI-SKYMONEY
      *
      *        入返金区分
               MOVE    "1"                 TO  SMEI-NYUHEN-KBN
      *
      *        入返金額
               MOVE    SPA-GMN-NYUKIN      TO  SMEI-NYUHEN-MONEY
      *
      *        入返金日
               IF    ( SPA-NAI-NYUKINYMD   =   SPACE )
                   MOVE    SPA-SYSYMD      TO  SMEI-NYUHEN-YMD
               ELSE
                   MOVE    SPA-NAI-NYUKINYMD
                                           TO  SMEI-NYUHEN-YMD
               END-IF
      *
      *        入金方法
               MOVE    SPA-GMN-NYKN-HOHO   TO  SMEI-NYUKIN-HOHO
      *
      *        状態区分
               MOVE    "2"                 TO  SMEI-JOUTAIKBN
      *
               PERFORM 800-MCNDATE-SEC
               MOVE    SMCNDATE-YMD        TO  SMEI-CREYMD
                                               SMEI-UPYMD
               MOVE    SMCNDATE-HMS        TO  SMEI-CREHMS
                                               SMEI-UPHMS
      *
               MOVE    SMEI-CREYMD         TO  SDAILYKEY-CREYMD
               MOVE    SMEI-CREHMS         TO  SDAILYKEY-CREHMS
               MOVE    SMEI-NYUHEN-YMD     TO  SDAILYKEY-BASEYMD
               PERFORM 800-DAILYKEY-SEC
               MOVE    SDAILYKEY-DAILYKEY  TO  SMEI-DAILYKEY
      *
               MOVE    SYUMEI-REC          TO  MCPDATA-REC
               MOVE    "tbl_syumei"        TO  MCP-TABLE
               MOVE    "key"               TO  MCP-PATHNAME
               PERFORM 910-DBINSERT-SEC
               IF    ( MCP-RC      NOT =   ZERO )
                   MOVE    "1004"          TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       49021-SYUMEI-INS-EXT.
           EXIT.
      *************************************************************
      *    収納明細更新処理
      *************************************************************
       49021-SYUMEI-UPD-SEC            SECTION.
      *
           MOVE    ZERO                TO  FLG-SYUMEI
           INITIALIZE                      SYUMEI-REC
           MOVE    SPA-HOSPNUM          TO  SMEI-HOSPNUM
           MOVE    SPA-S01-NYUGAIKBN   TO  SMEI-NYUGAIKBN
           MOVE    SPA-S01-PTID        TO  SMEI-PTID
           MOVE    SPA-S01-DENPNUM     TO  SMEI-DENPNUM
           MOVE    SPA-NAI-DENPEDANUM  TO  SMEI-DENPEDANUM
           MOVE    SYUMEI-REC          TO  MCPDATA-REC
           MOVE    "tbl_syumei"        TO  MCP-TABLE
           MOVE    "key"               TO  MCP-PATHNAME
           PERFORM 910-DBSELECT-SEC
           IF    ( MCP-RC              =    ZERO )
               MOVE    MCPDATA-REC     TO  SYUMEI-REC
           ELSE
               MOVE    1               TO  FLG-SYUMEI
               INITIALIZE                  SYUMEI-REC
           END-IF
      *
           MOVE    "tbl_syumei"        TO  MCP-TABLE
           MOVE    "key"               TO  MCP-PATHNAME
           PERFORM 910-DBCLOSECURSOR-SEC
      *
           IF    ( FLG-SYUMEI      NOT =   ZERO )
               MOVE    "1005"          TO  SPA-ERRCD
           ELSE
      *
               MOVE    SMEI-NYUHEN-YMD     TO  WRK-OLD-NYUHEN-YMD
      *
      *        請求金額
               IF    ( SPA-NAI-PROC-FLG    = 1 OR 2 )
                   MOVE    SPA-GMN-CHOSEIIN
                                           TO  SMEI-SKYMONEY
               ELSE
                   MOVE    ZERO            TO  SMEI-SKYMONEY
               END-IF
      *
      *        入返金額
               MOVE    SPA-GMN-NYUKIN      TO  SMEI-NYUHEN-MONEY
      *
      *        入返金日
               IF    ( SPA-NAI-NYUKINYMD   =   ZERO )
                   MOVE     SPA-SYSYMD     TO  SMEI-NYUHEN-YMD
               ELSE
                   MOVE     SPA-NAI-NYUKINYMD
                                           TO  SMEI-NYUHEN-YMD
               END-IF
      *
      *        入金方法
               IF    ( SPA-GMN-NYUKIN      >   ZERO )
                OR   ( SMEI-DENPEDANUM     =   1 )
                   MOVE  SPA-GMN-NYKN-HOHO TO  SMEI-NYUKIN-HOHO
               ELSE
                   MOVE   "00"             TO  SMEI-NYUKIN-HOHO
               END-IF
      *
      *        伝票状態区分
               EVALUATE    SPA-NAI-PROC-FLG
               WHEN    1
                   IF    ( SPA-GMN-CHOSEIIN   <=   ZERO )
                       MOVE    SPACE   TO  SMEI-JOUTAIKBN
                   ELSE
                       MOVE    "1"     TO  SMEI-JOUTAIKBN
                   END-IF
               WHEN    2
               WHEN    4
                   CONTINUE
               WHEN    OTHER
                   IF    ( SPA-GMN-NYUKIN     >=   ZERO )
                       MOVE    "2"     TO  SMEI-JOUTAIKBN
                   ELSE
                       MOVE    "4"     TO  SMEI-JOUTAIKBN
                   END-IF
               END-EVALUATE
      *
               PERFORM 800-MCNDATE-SEC
               MOVE    SMCNDATE-YMD        TO  SMEI-UPYMD
               MOVE    SMCNDATE-HMS        TO  SMEI-UPHMS
      *
               IF    ( SMEI-NYUHEN-YMD     NOT =   WRK-OLD-NYUHEN-YMD )
                   MOVE    SMEI-UPYMD          TO  SDAILYKEY-CREYMD
                   MOVE    SMEI-UPHMS          TO  SDAILYKEY-CREHMS
                   MOVE    SMEI-NYUHEN-YMD     TO  SDAILYKEY-BASEYMD
                   PERFORM 800-DAILYKEY-SEC
                   MOVE    SDAILYKEY-DAILYKEY  TO  SMEI-DAILYKEY
               END-IF
      *
               MOVE    SYUMEI-REC          TO  MCPDATA-REC
               MOVE    "tbl_syumei"        TO  MCP-TABLE
               MOVE    "key"               TO  MCP-PATHNAME
               PERFORM 910-DBUPDATE-SEC
               IF    ( MCP-RC          NOT =   ZERO )
                   MOVE    "1006"          TO  SPA-ERRCD
               END-IF
           END-IF
      *
           .
       49021-SYUMEI-UPD-EXT.
           EXIT.
      **************************************************************
      *    処理区分変更処理
      **************************************************************
       430-SYORIKBN-SEC                SECTION.
      *
           IF    ( SPA-NAI-HEAD-DENPJTIKBN NOT =   "3" )
      *
               EVALUATE SPA-S01-SYORINO
      *
               WHEN    CONST-PROC-NYUKIN
               MOVE    CONST-PROC-RRKSYU   TO  SPA-S01-SYORINO
      *
               WHEN    CONST-PROC-RRKSYU
               MOVE    CONST-PROC-SYOKAI   TO  SPA-S01-SYORINO
      *
               WHEN    CONST-PROC-SYOKAI
               MOVE    CONST-PROC-NYUKIN   TO  SPA-S01-SYORINO
      *
               WHEN    OTHER
               MOVE    CONST-PROC-SYOKAI   TO  SPA-S01-SYORINO
               END-EVALUATE
      *
               PERFORM 4200-SPA-SET-SEC
      *
               MOVE    1               TO  FLG-SMEILIST1-ROW
      *
           END-IF
      *
           .
       430-SYORIKBN-EXT.
           EXIT.
      **************************************************************
      *    予約登録へ
      **************************************************************
       480-YOYAKU-SEC             SECTION.
      *
      *    戻り時、画面を元へ戻す
           MOVE    SPACE               TO  LINKAREA
           MOVE    SPA-KYOUTU          TO  LNK-COMMON
           MOVE    SPA-S03             TO  LNK-SCRSPA
      *    画面移行用のパラメタ変更
           MOVE    "S03"               TO  SPA-MOTOPG
           MOVE    "S03"               TO  SPA-MOTOPG3
           MOVE    "Y01"               TO  SPA-SAKIPG
           INITIALIZE                  SPA-KYOTUKEY
           MOVE    SPA-KYOUTU          TO  LNK-KYOUTU
      *
           MOVE   "CHANGE"             TO  MCP-PUTTYPE
           MOVE   "Y01"                TO  MCP-WINDOW
      *
           PERFORM 900-PUT-WINDOW
      *
           MOVE    1                   TO  FLG-END
      *
           .
       480-YOYAKU-EXT.
           EXIT.
      *
      **************************************************************
      *    受付一覧へ
      **************************************************************
       490-UKEICHI-SEC             SECTION.
      *
      *    戻り時、画面を元へ戻す
           MOVE    SPACE               TO  LINKAREA
           MOVE    SPA-KYOUTU          TO  LNK-COMMON
           MOVE    SPA-S03             TO  LNK-SCRSPA
      *
      *    画面移行用のパラメタ変更
           MOVE    "S03"               TO  SPA-MOTOPG
           MOVE    "U01"               TO  SPA-SAKIPG
           MOVE    SPA-KYOUTU          TO  LNK-KYOUTU
      *
           MOVE   "CHANGE"             TO  MCP-PUTTYPE
           MOVE   "U01    "            TO  MCP-WINDOW
      *
           PERFORM 900-PUT-WINDOW
      *
           MOVE    1                   TO  FLG-END
      *
           .
       490-UKEICHI-EXT.
           EXIT.
      *
      *
      *****************************************************************
      *    入金コンボチェック処理
      *****************************************************************
       4200-NYKN-HOHOLST-CHK-SEC       SECTION.
      *
           MOVE    SPACE               TO  WRK-CMB-ITM-G
      *
           PERFORM VARYING IDX-CMB FROM    1   BY  1
               UNTIL ( IDX-CMB     >   SPA-GMN-NYKN-HOHO-MAX )
                OR   ( WRK-CMB-CD  =   SPA-GMN-NYKN-HOHOL (IDX-CMB) )
      *
               CONTINUE
      *
           END-PERFORM
      *
           IF    ( IDX-CMB     <=  SPA-GMN-NYKN-HOHO-MAX )
               MOVE    SPA-GMN-NYKN-HOHOLST (IDX-CMB)
                                       TO  WRK-CMB-ITM
               MOVE    SPA-NAI-NYKN-HOHOLST (IDX-CMB)
                                       TO  WRK-CMB-ITM2
           END-IF
      *
           .
       4200-NYKN-HOHOLST-CHK-EXT.
           EXIT.
      *****************************************************************
      *    請求書発行区分コンボチェック処理
      *****************************************************************
       4200-SKYPRTLST-CHK-SEC          SECTION.
      *
           MOVE    SPACE               TO  WRK-CMB-ITM-G
      *
           PERFORM VARYING IDX-CMB FROM    1   BY  1
               UNTIL ( IDX-CMB     >   SPA-GMN-SKYPRT-MAX )
                OR   ( WRK-CMB-CD  =   SPA-GMN-SKYPRTL (IDX-CMB) )
      *
               CONTINUE
      *
           END-PERFORM
      *
           IF    ( IDX-CMB     <=  SPA-GMN-SKYPRT-MAX )
               MOVE    SPA-GMN-SKYPRTLST (IDX-CMB)
                                       TO  WRK-CMB-ITM
           END-IF
      *
           .
       4200-SKYPRTLST-CHK-EXT.
           EXIT.
      *****************************************************************
      *    明細書発行区分コンボチェック処理
      *****************************************************************
       4200-SRYPRTLST-CHK-SEC          SECTION.
      *
           MOVE    SPACE               TO  WRK-CMB-ITM-G
      *
           PERFORM VARYING IDX-CMB FROM    1   BY  1
               UNTIL ( IDX-CMB     >   SPA-GMN-SRYPRT-MAX )
                OR   ( WRK-CMB-CD  =   SPA-GMN-SRYPRTL (IDX-CMB) )
      *
               CONTINUE
      *
           END-PERFORM
      *
           IF    ( IDX-CMB     <=  SPA-GMN-SRYPRT-MAX )
               MOVE    SPA-GMN-SRYPRTLST (IDX-CMB)
                                       TO  WRK-CMB-ITM
           END-IF
      *
           .
       4200-SRYPRTLST-CHK-EXT.
           EXIT.
      *****************************************************************
      *    ＳＰＡ編集処理
      *****************************************************************
       4200-SPA-SET-SEC                SECTION.
      *
           MOVE    SPA-GMN-SKYPRT          TO  WRK-SKYPRTKBN
           PERFORM 310-SPA-SET-SEC
           MOVE    WRK-SKYPRTKBN           TO  SPA-GMN-SKYPRT
           PERFORM 4101-SKYPRT-CHK-SEC
      *
           MOVE    SPA-GMN-SRYPRT          TO  WRK-SRYPRTKBN
           PERFORM 310-SPA-SET-SEC
           MOVE    WRK-SRYPRTKBN           TO  SPA-GMN-SRYPRT
           PERFORM 4101-SRYPRT-CHK-SEC
      *
           .
       4200-SPA-SET-EXT.
           EXIT.
      *************************************************************
      *    エラーメッセージ表示理
      *************************************************************
       510-ERRSET-SEC                  SECTION.
      *
           MOVE    SPACE               TO  SPA-ERRMSG
      *
           EVALUATE    SPA-ERRCD
           WHEN    "0003"
               MOVE    "入金合計額が請求額を超えています"
                                       TO  SPA-ERRMSG
           WHEN    "0004"
               MOVE    "入金方法を入力して下さい"
                                       TO  SPA-ERRMSG
           WHEN    "0005"
               MOVE    "選択番号入力エラー"
                                       TO  SPA-ERRMSG
           WHEN    "0006"
               MOVE    "選択番号が未入力です"
                                       TO  SPA-ERRMSG
           WHEN    "0007"
               MOVE    "入金額を入力してください"
                                       TO  SPA-ERRMSG
           WHEN    "0008"
