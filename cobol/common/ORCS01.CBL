                 IF   ( COMB-KOH1HKNNUM  =   "972" )
                 OR   ( COMB-KOH2HKNNUM  =   "972" )
                 OR   ( COMB-KOH3HKNNUM  =   "972" )
                 OR   ( COMB-KOH4HKNNUM  =   "972" )
