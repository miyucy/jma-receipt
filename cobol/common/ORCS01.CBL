                                               HKN-KZK-GAIKFTNRATE
                                               HKN-TSY-GAIKFTNRATE
                                               HKN-TNK-GAIKFTNRATE
                       END-IF 
                   END-IF
               END-IF
           END-IF.
      *    義務教育就学前
           IF      LNK-SYU-SRYYMD       >=      "20080401"
               IF      FLG-SYUGAKUMAE    =       1
                   IF   COMB-HKNNUM       =     "060"
                       IF   COMB-HOJOKBN  =     "0"  OR  "1"
                                                     OR  "4"
