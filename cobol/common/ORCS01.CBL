                      OR ((LNK-SYU-KOH1HKNNUM  =  SYU-KOH2HKNNUM ) AND
                          (COMB-KOH1ID         =  WRK-COMB-KOH2ID))
                      OR ((LNK-SYU-KOH1HKNNUM  =  SYU-KOH3HKNNUM ) AND
                          (COMB-KOH1ID         =  WRK-COMB-KOH3ID))
                      OR ((LNK-SYU-KOH1HKNNUM  =  SYU-KOH4HKNNUM ) AND
