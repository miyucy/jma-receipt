                   OR  (WK-NYUSKJRYYKBN-TOKU(2)  =  "2")
                   OR  (WK-NYUSKJRYYKBN-TOKU(3)  =  "2")
                   OR  (WK-NYUSKJRYYKBN-TOKU(4)  =  "2")
