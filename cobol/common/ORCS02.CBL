                        COMPUTE  WK-ZENGAKU-YAK = WK-ZENGAKU 
                                                - LNK-S02-SYUTEN
                        COMPUTE  WK-ZENGAKU-SYU =
                          (WK-ZENGAKU-SYU * 12.0 * WK-KSNRATE)
                                                 + 0.5
                        COMPUTE  WK-ZENGAKU-YAK =
                          (WK-ZENGAKU-YAK * 12.0)
                                                 + 0.5
                        COMPUTE  WK-ZENGAKU     = WK-ZENGAKU-SYU
