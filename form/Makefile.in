clean: 

distclean: 
	-rm -f Makefile

filecheck:
	@(for i in $(FORM_files) ;do echo "make	$${i}"; done ;\
	find . \
	-name "." -o \
	-name "CVS" -prune -o \
	-name "Makefile*" -o \
	-name ".cvsignore" -o \
	-name "*.bak" -o \
	-name "build-stamp" -o \
	-printf "file\t%f\n") \
	|sort +1 |uniq -u -f1 

install: all
	$(INSTALL) -d $(formdir)
	$(INSTALL_DATA) $(FORM_files) $(formdir)
